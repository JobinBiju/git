<script type="text/javascript">
    var dataGrid;
   
    $(function() {
        dataGrid = $('#tenderListGrid').datagrid({
            url : '/modules/analysis/userManageBySourceList.html?sourceURL=${sourceURL!}&startTime=${startTime!}&endTime=${endTime!}',
            fit : true,
            fitColumns : true,
            border : false,
            pagination : true,
            idField : 'id',
            pageSize : 10,
            pageNums :1,
            pageList : [ 10, 20, 30, 40, 50 ],
            checkOnSelect : false,
            selectOnCheck : false,
            nowrap : true,
			singleSelect:true,
			showFooter: true,
            columns : [ [
			  { field: 'userId', title: '用户id', width: 60, sortable:false },
			  { field: 'userName', title: '用户名', width: 80, sortable:false },
			  { field: 'realName', title: '真实姓名', width: 80, sortable:false },
			  { field: 'mobilePhone', title: '手机号', width: 80, sortable:false },
              { field: 'addTime', title: '注册时间', width: 150, sortable:false,fixed:true,formatter: function(value,row,index){
            	  return getLocalTime(value,4);
              	} 
              }, 
              { field: 'sourceURL', title: '用户来源', width: 150, sortable:false },
              { field: 'registerWay', title: '注册页面', width: 150, sortable:false }
            
            ] ],
            onLoadSuccess : function() {
				$('#searchForm table').show();
				parent.$.messager.progress('close');
				$(this).datagrid('tooltip');
			}
        });
    });
  //借款管理中的excel导出
</script>

