
package com.rongdu.p2psys.borrow.model;

import java.util.Date;

/**
 * @描述: 手机端借款信息展示
 * @作者:shenguoping@ddtkj.com
 * @创建时间:  2015-2-3   
 */
public class MobileBorrowModel {
	
	private long id;
	private double apr;		//年利率
	private int time_limit; //借款期限
	private int borrow_time_type; //是否天标 1是天标
	private double account; //借款金额
	private int styleName; //还款方式
	private long user_id; 
	private String name; //借款标题
	private double scales; //进度
	private int status; //状态
	private Date fixed_time; //上线时间
	private Date add_time; //发布时间
	private Date verify_time; //初审时间
	private int valid_time; //有效期限
	private double account_yes; //借款入账金额
	private String style; //还款方式字符串，便于手机端处理
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public double getApr() {
		return apr;
	}
	public void setApr(double apr) {
		this.apr = apr;
	}
	public int getTime_limit() {
		return time_limit;
	}
	public void setTime_limit(int time_limit) {
		this.time_limit = time_limit;
	}
	public int getBorrow_time_type() {
		return borrow_time_type;
	}
	public void setBorrow_time_type(int borrow_time_type) {
		this.borrow_time_type = borrow_time_type;
	}
	public double getAccount() {
		return account;
	}
	public void setAccount(double account) {
		this.account = account;
	}
	public String getStyle() {
		return style;
	}
	public void setStyle(String style) {
		this.style = style;
	}
	public void setStyleName(int styleName) {
		this.styleName = styleName;
	}
	public long getUser_id() {
		return user_id;
	}
	public void setUser_id(long user_id) {
		this.user_id = user_id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getScales() {
		return scales;
	}
	public void setScales(double scales) {
		this.scales = scales;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public Date getFixed_time() {
		return fixed_time;
	}
	public void setFixed_time(Date fixed_time) {
		this.fixed_time = fixed_time;
	}
	public Date getAdd_time() {
		return add_time;
	}
	public void setAdd_time(Date add_time) {
		this.add_time = add_time;
	}
	public Date getVerify_time() {
		return verify_time;
	}
	public void setVerify_time(Date verify_time) {
		this.verify_time = verify_time;
	}
	public int getValid_time() {
		return valid_time;
	}
	public void setValid_time(int valid_time) {
		this.valid_time = valid_time;
	}
	public double getAccount_yes() {
		return account_yes;
	}
	public void setAccount_yes(double account_yes) {
		this.account_yes = account_yes;
	}

}
