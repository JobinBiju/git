
package com.ddtkj.p2psys.redpacket.exception;

import com.rongdu.common.exception.BussinessException;

/**
 * @描述:
 * @作者:shenguoping@ddtkj.com
 * @创建时间:  2015-6-2   
 */
public class RpWeixinDataException extends BussinessException {
	
	private static final long serialVersionUID = 4078133174344776822L;

	public RpWeixinDataException() {
		super();
	}

	public RpWeixinDataException(String message) {
		super(message);
	}

	public RpWeixinDataException(String message, int type) {
		super(message, type);
	}

	public RpWeixinDataException(String msg, RuntimeException ex) {
		super(msg, ex);
	}

	public RpWeixinDataException(String message, String url) {
		super(message, url);
	}
}
