package com.ddtkj.p2psys.credit.exception;

import com.rongdu.common.exception.BussinessException;

/**
 * 债权转让异常
 * 
 * @author xx
 * @version 2.0
 * @since 2014年3月4日
 */
public class CreditException extends BussinessException {

	private static final long serialVersionUID = 1129253629501700179L;

	public CreditException() {
		super();
	}

	public CreditException(String message) {
		super(message);
	}

	public CreditException(String message, String url) {
		super(message, url);
	}

	public CreditException(String message, int type) {
		super(message, type);
	}

}
