package com.ddtkj.p2psys.credit.model;


/**
 * @描述：手机端 已购入MODEL
 * @作者：yujunfei@ddtkj.com
 * @创建时间：2015年1月12日 
 */
public class CreditBuyInMobileModel {
	private long id ;   //记录id  债权ID
	private String  borrowName ; //借款标题
    private double borrowApr  ;                 // 年利率
    private double creditCpoisValue ;                   // 每份价值 
    private double buyCopies   ;             // 购买多少份
    private double creditPrice  ;             // 转让总价(元)
	private double buyAccount  ;                  // 交易金额
	private int buyCreditBalance  ;                  // 盈亏  0 盈 1 亏
	private String buytime   ;                // 转入时间
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getBorrowName() {
		return borrowName;
	}
	public void setBorrowName(String borrowName) {
		this.borrowName = borrowName;
	}
	public double getBorrowApr() {
		return borrowApr;
	}
	public void setBorrowApr(double borrowApr) {
		this.borrowApr = borrowApr;
	}
	public double getCreditCpoisValue() {
		return creditCpoisValue;
	}
	public void setCreditCpoisValue(double creditCpoisValue) {
		this.creditCpoisValue = creditCpoisValue;
	}
	public double getBuyCopies() {
		return buyCopies;
	}
	public void setBuyCopies(double buyCopies) {
		this.buyCopies = buyCopies;
	}
	public double getCreditPrice() {
		return creditPrice;
	}
	public void setCreditPrice(double creditPrice) {
		this.creditPrice = creditPrice;
	}
	public double getBuyAccount() {
		return buyAccount;
	}
	public void setBuyAccount(double buyAccount) {
		this.buyAccount = buyAccount;
	}
	public int getBuyCreditBalance() {
		return buyCreditBalance;
	}
	public void setBuyCreditBalance(int buyCreditBalance) {
		this.buyCreditBalance = buyCreditBalance;
	}
	public String getBuytime() {
		return buytime;
	}
	public void setBuytime(String buytime) {
		this.buytime = buytime;
	}
}
