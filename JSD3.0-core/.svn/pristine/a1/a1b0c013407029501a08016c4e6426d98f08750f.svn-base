	@SuppressWarnings("deprecation")
	@Override
	public PageDataList<CreditSwingOutModel> getSwingOutCreditTender(User user,int page,int size) {
		// 获取用户所有投的标，并且未完全还款的
		logger.info("进入可转让业务逻辑");
		List<BorrowTender> tendList = borrowTenderDao.getSwingOutTender(user);
		List<CreditSwingOutModel> csomList = new ArrayList<CreditSwingOutModel>() ;
		for (BorrowTender borrowTender : tendList) {
			CreditSwingOutModel csom =null ;
 			// 获取当前标需要还的还款期数
 			Borrow borrow = borrowDao.find(borrowTender.getBorrow().getId()) ;
 			int currPeriod=0;
 			//JSDP-225 gjh 2015-7-21 start
 			Date endTime = null;
 			if(Borrow.TYPE_NETWORK==borrow.getType()||Borrow.TYPE_MORTGAGE_ENERGY==borrow.getType()){
 				currPeriod=borrowCollectionDao.getCurrPeriod(borrowTender.getBorrow().getId(),borrowTender.getId());
 				endTime = DateUtil.getDate(DateUtil.getTime(borrowTender.getAddTime())+"");
 				if(Borrow.BORROW_TIME_TYPE_ISDAY == borrow.getBorrowTimeType()){
 					endTime.setDate(endTime.getDate() + borrow.getPeriod() + 1);
 				}else{
 					endTime.setMonth(endTime.getMonth() + borrow.getPeriod() + 1);
 				}
 			}else{
 				currPeriod = borrowRepaymentDao.getCurrPeriod(borrowTender.getBorrow().getId()); 
 				VerifyLog log = verifyLogDao.findByType(borrow.getId(), "borrow", 2);
 				endTime = DateUtil.getDate(DateUtil.getTime(log.getTime())+"");
 				if(Borrow.BORROW_TIME_TYPE_ISDAY == borrow.getBorrowTimeType()){
 					endTime.setDate(endTime.getDate() + borrow.getPeriod() + 1);
 				}else{
 					endTime.setMonth(endTime.getMonth() + borrow.getPeriod() + 1);
 				}
 			}
 			logger.info("currPeriod-====="+currPeriod);
			// 获取当前用户当前标需要还的待收
			BorrowCollection  bcm = borrowCollectionDao.getThisPeriod(user, borrow, currPeriod,borrowTender.getId()) ;
			logger.info("bcm-====="+bcm);
			//判断代收是否为空
			if(bcm != null){
				csom = getSwingOutBuyTend(borrowTender, user,1,BorrowCollectionModel.instance(bcm),borrow,currPeriod) ;
				logger.info("csom-====="+csom);
				if(csom != null&&csom.getCreditCopies()>0){
					csom.setType(1);
					csom.setCollectionId(bcm.getId());
					csom.setBorrowRepaymentEndTime(endTime);
					//JSDP-225 gjh 2015-7-21 end
					csomList.add(csom);
				}
			}
			
		}
		List<CreditSwingOutModel> resultlist = csomList.subList(((page-1)*size), ((page*size)>csomList.size()?csomList.size():(page*size))) ;
		PageDataList<CreditSwingOutModel> csolist = new PageDataList<CreditSwingOutModel>(new Page(csomList.size(),page,size),resultlist) ;
		return csolist;
	}
