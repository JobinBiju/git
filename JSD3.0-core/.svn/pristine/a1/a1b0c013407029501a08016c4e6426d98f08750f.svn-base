	// JSDP-221  wcw 2015-07-14 start
	public void creditAssignmentDataModify(CreditAssignment creditAssignment){
		QueryParam qp=new QueryParam();
		qp.addParam("creditAssignment", creditAssignment);
		List<CreditLog> creditLogList=creditLogDao.findByCriteria(qp);
		for(CreditLog cl:creditLogList){
			List<CollectionCredit> list=collectionCreditDao.findCollectionByCredit(cl.getId());
			if(list.size()>0){
				for(CollectionCredit cc:list){
					BorrowCollection bc=cc.getBorrowCollection();
					borrowCollectionDao.delete(bc.getId());
					//collectionCreditDao.delete(cc.getId());
				}
			}
	    }
		for(CreditLog cl:creditLogList){
			List<BorrowCollection> oldCollectList = null ;
			CreditAssignment oldCa = null ;
			CreditAssignment newCa=null;
			Borrow borrow=creditAssignment.getBorrow();
			if(creditAssignment.getCaType()==1){
				oldCollectList = borrowCollectionDao.getMemberCollectionList(creditAssignment.getUser(), borrow,creditAssignment.getTender().getId());
				oldCa=creditAssignment;
			}
			// 购买者待收
			InstantLiveCreditWorker ic=new InstantLiveCreditWorker(creditAssignment, cl);
			if(oldCollectList!=null&&oldCollectList.size()>0){
				List<BorrowCollection> collectList = ic.createCollectionList(oldCollectList,borrow.getStyle(),creditAssignment,cl);
				if(collectList.size()>0){
					borrowCollectionDao.save(collectList);
					for (BorrowCollection borrowCollection : collectList) {
						CollectionCredit cc = new CollectionCredit() ;
						cc.setAddIp("");
						cc.setAddTime(new Date());
						cc.setBorrowCollection(borrowCollection);
						cc.setBuyCa(creditAssignment);
						cc.setOldCa(oldCa);
						cc.setNewCa(newCa);
						cc.setCreditLog(cl);
						collectionCreditDao.save(cc);
					}
				}
			}
		}
	}
	
	public List<CreditAssignment> creditAssignmentData(CreditAssignment creditAssignment){
		QueryParam param=new QueryParam();
		param.addParam("creditStartPeriod",Operators.GT, creditAssignment.getCreditStartPeriod());
		param.addParam("status", creditAssignment.getStatus());
		List<CreditAssignment> list=creditAssignmentDao.findByCriteria(param);
		return list;
	}
	
