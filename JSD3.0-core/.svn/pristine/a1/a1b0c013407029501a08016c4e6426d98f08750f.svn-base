			    currPeriod = borrowRepaymentDao.getCurrPeriod(borrowTender.getBorrow().getId(),borrowTender.getId()); 
			}else{
				currPeriod = borrowRepaymentDao.getCurrPeriod(borrowTender.getBorrow().getId()); 
