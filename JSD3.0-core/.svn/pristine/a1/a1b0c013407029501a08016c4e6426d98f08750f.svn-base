	@Override
	public void creditAssignment(CreditAssignment c){
		if(c.getId()==160){
			@SuppressWarnings("unused")
			int a=0;
		}
		logger.info("进入新增当天债权价值、价格核心业务逻辑开始，债权ID："+c.getId());
		logger.info("creditAssignment====="+c);
		// 获取当前用户当前标需要还的待收
		BorrowCollectionModel bcm=new BorrowCollectionModel();
		double collCapital =0;
		// 获取当前标需要还的还款期数
		int currPeriod=0;
		logger.info("currPeriod==="+currPeriod);
		if(c.getCaType()==1){
			logger.info("原投资转让===");
 			BorrowTender borrowTender = borrowTenderDao.find(c.getTender().getId());
 			if(Borrow.TYPE_NETWORK==c.getBorrow().getType()||Borrow.TYPE_MORTGAGE_ENERGY==c.getBorrow().getType()){
				currPeriod=borrowCollectionDao.getCurrPeriod(borrowTender.getBorrow().getId(),borrowTender.getId());
			}else{
				currPeriod = borrowRepaymentDao.getCurrPeriod(borrowTender.getBorrow().getId()); 
			}
 			// 获取当前标需要还的还款期数
 			Borrow borrow = borrowDao.find(borrowTender.getBorrow().getId()) ;
			// 获取当前用户当前标需要还的待收
 			BorrowCollection b=borrowCollectionDao.getThisPeriod(c.getUser(), borrow, currPeriod,borrowTender.getId());
 			if(b!=null){
			  bcm =BorrowCollectionModel.instance(b) ;
			}
 			//JSDP-210 wcw 2015-07-4 start  剩余待收本金=原待收本金总额-已还款本金总额
 			double collCapitalOne=0;
 			if(c.getSoldAccount()>0){
 				collCapitalOne = borrowCollectionDao.getCollectionRepayCapitalTotal(borrow, borrowTender.getId(), 0,2); // 原待收本金
 			}else{
 				collCapitalOne = borrowCollectionDao.getCollectionRepayCapitalTotal(borrow, borrowTender.getId(), 0,0); // 原待收本金
 			}
			double collCapitalTwo = borrowCollectionDao.getCollectionRepayCapitalTotal(borrow, borrowTender.getId(), 1,0); // 已还款本金
			collCapital=collCapitalOne-collCapitalTwo;
			//JSDP-210 wcw 2015-07-4 end
			logger.info("collCapital==="+collCapital);
		}
		if(c.getCaType()==2){
			// 获取当前用户当前标需要还的待收
			currPeriod =borrowCollectionDao.getMinPeriodByCredit(c.getCreditLog()); 
			bcm = borrowCollectionDao.getThisPeriodByCredit(c.getCreditLog(), currPeriod);
			collCapital = borrowCollectionDao.getCapitalTotalByCredit(c.getCreditLog()) ; // 剩余待收本金
			QueryParam queryParam=new QueryParam();
			queryParam.addParam("creditAssignment", c);
			List<CreditLog> creditLogList=creditLogDao.findByCriteria(queryParam);
			double yesCollCapitalTotal=0;
			double yesCollCapital=0;
			for(CreditLog cl:creditLogList){
				yesCollCapital = borrowCollectionDao.getCapitalTotalByCredit(cl,"1") ; 
				yesCollCapitalTotal=BigDecimalUtil.add(yesCollCapitalTotal,yesCollCapital);// 已还本金
			}
			collCapital=BigDecimalUtil.sub(collCapital,yesCollCapitalTotal);// 剩余待收本金
			logger.info("collCapital==="+collCapital); 
		}
		// 判断当前时间与还款时间的时间差
		logger.info("bcm==="+bcm);
		if(collCapital>0){
			if(bcm!=null){
				long day = 0;
				if(c.getBorrow().getBorrowTimeType()!=Borrow.BORROW_TIME_TYPE_ISDAY){
					day = DateUtil.getSubDay(DateUtil.rollMon(bcm.getRepaymentTime(),-1),new Date()) ;
				}else{
					day = DateUtil.getSubDay(DateUtil.rollDay(bcm.getRepaymentTime(),-1),new Date()) ;
				}
				logger.info("day==="+day);
				// 一次性还款 不分期
				long haveday = (int)DateUtil.getSubDay(DateUtil.rollMon(bcm.getRepaymentTime(),-1), bcm.getRepaymentTime()) ;
				if(day>0 && day<haveday){  // 小于0 并且 相差大于30天的，当前时间不处于转让第一期
					haveday = day ;
				}else{
					haveday = 0 ;
				}
				if(CreditCalculatorWorker.STYLE_ONETIME_REPAYMENT == c.getBorrow().getStyle()){
					haveday = day ;
				}
				logger.info("haveday==="+haveday);
				if(haveday>=0){
					// 计算本期应计天数
					CreditCalculatorUtil ccl = new CreditCalculatorWorker(collCapital, c.getBorrow().getApr(), 1,(int)haveday,1, c.getBorrow().getTimeLimit(), c.getBorrow().getStyle(), bcm.getInterest(),bcm.getRepaymentTime(),c.getCreditCopies(),c.getBorrow().getBorrowTimeType()).getCalculator() ;
					//转出者本期所得利息
					double interest=ccl.getCopiesInsterest();
					//该债权价值
				    double caTotalValue=BigDecimalUtil.decimal(BigDecimalUtil.add(interest,collCapital),2);
				    //该债权价格
				    double caTotalPrice=BigDecimalUtil.decimal(BigDecimalUtil.mul(c.getCreditDis(), caTotalValue), 2);
				    //剩余份数
				    double caSurplusCopies=BigDecimalUtil.add(c.getCreditCopies(),-c.getSellCopies());
				    logger.info("caTotalPrice==="+caTotalPrice);
				    logger.info("caTotalValue==="+caTotalValue);
				    if(caTotalPrice>0&&caTotalValue>0){
					    CaLog caLog=new CaLog(c, caTotalValue, caTotalPrice, c.getCreditDis(), caSurplusCopies, c.getAddTime(), new Date());
					    caLogDao.save(caLog);
				    }
				    CreditAssignment ca=creditAssignmentDao.find(c.getId());
				    logger.info("ca==="+ca);
				    if(ca!=null){
					    ca.setCreditValue(caTotalValue);
					    ca.setCreditPrice(caTotalPrice);
					    ca.setHaveInsterest(interest);
					    creditAssignmentDao.update(ca);
				    }
			    }
			}
	    }else{
	    	 logger.info("剩余待收本金===="+collCapital);
	    }
	}
	
	//JSDP-221 wcw 2015-07-17 start
	@Override
	public void unFullCreditAssignmentEvent(CreditAssignment ca){
		logger.info("进入未满债权处理核心业务逻辑开始，债权ID："+ca.getId());
		logger.info("creditAssignment====="+ca);
		if(ca.getCaType()==1){
			logger.info("原投资转让===");
		    QueryParam param=new QueryParam();
	        param.addParam("creditAssignment", ca);
	    	List<CreditLog> creditLogList=creditLogDao.findByCriteria(param);
		    for(int i=ca.getCreditStartPeriod()-1;i<=ca.getBorrow().getPeriod();i++){
		    	//本期原待收
		    	List<BorrowCollection> oldList=borrowCollectionDao.getMemberCollectionList(ca.getUser(), ca.getBorrow(), ca.getTender().getId(), "", "2", i+"");
		    	//本期因该债权未满补上待收
		    	Map<String, Object> map=borrowCollectionDao.getMemberCollectionSum(ca.getUser(), ca.getBorrow(), ca.getTender().getId(), "", "0", i+"");
		    	double interestSum=NumberUtil.getDouble(map.get("interestSum")+"");
		    	double repaymentAccountSum=NumberUtil.getDouble(map.get("repaymentAccountSum")+"");
		    	if(interestSum>0&&repaymentAccountSum>0){
		    		//更新债权状态为结束，债权存活期为到下一个还款日之前的间隔
					ca.setStatus(CreditAssignment.CREDIT_FINISH);
					creditAssignmentDao.update(ca);
		    	}
		    	if(oldList.size()>0){
		        	logger.info("原投资转让核心处理===");
			    	BorrowCollection oldBc=oldList.get(0);
			        //债权转让出待收
			        double newCapital=0;
			        double newInterest=0;
			        double newRepaymentAccount=0;
				    double newCapitalTotal=0;
				    double newInterestTotal=0;
			        double newRepaymentAccountTotal=0;
				    for(CreditLog cl:creditLogList){
				    	BorrowCollection bcn=borrowCollectionDao.getCollectionByCredit(cl,i+"","","");
				    	newCapital=bcn.getCapital();
				    	newInterest=bcn.getInterest();
				    	newRepaymentAccount=bcn.getRepaymentAccount();
			    		newCapitalTotal+=newCapital;
			    		newInterestTotal+=newInterest;
			    		newRepaymentAccountTotal+=newRepaymentAccount;
				    }
				    //剩余未转出债权待收处理
				    double remainInterest=BigDecimalUtil.sub(BigDecimalUtil.sub(oldBc.getInterest(), newInterestTotal),interestSum);
				    double remainRepaymentAccount=BigDecimalUtil.sub(BigDecimalUtil.sub(oldBc.getRepaymentAccount(), newRepaymentAccountTotal),repaymentAccountSum);
				    if(remainInterest>0&&remainRepaymentAccount>0&&ca.getSoldAccount()>0){
				    	BorrowCollectionModel bcml=new BorrowCollectionModel();
					    BorrowCollection bc=bcml.fillCollection(oldBc, BigDecimalUtil.sub(oldBc.getCapital(), newCapitalTotal),
			    		BigDecimalUtil.sub(oldBc.getInterest(), newInterestTotal), 
			    		BigDecimalUtil.sub(oldBc.getRepaymentAccount(), newRepaymentAccountTotal));
				        borrowCollectionDao.save(bc);
				    }else{
				    	logger.info("投资转出，债权ID为"+ca.getId()+",剩余利息===="+remainInterest+",剩余待收金额=="+remainRepaymentAccount);
				    }
			    }
		    }
		}
		if(ca.getCaType()==2){
			logger.info("债权多次转让===");
			QueryParam param=new QueryParam();
	        param.addParam("creditAssignment", ca);
	    	List<CreditLog> creditLogList=creditLogDao.findByCriteria(param);
			for(int i=ca.getCreditStartPeriod()-1;i<=ca.getBorrow().getPeriod();i++){
		        //债权再转让原待收
				CreditLog oldCl=ca.getCreditLog();
				//本期原待收
				BorrowCollection oldBc=new BorrowCollection();
				BorrowCollection newBc=new BorrowCollection();
				if(ca.getSoldAccount()>0){
				    oldBc=borrowCollectionDao.getCollectionByCredit(oldCl,i+"","2","");
				  //本期因债权未满补上待收
				    newBc=borrowCollectionDao.getCollectionByCredit(oldCl,i+"","0","");
					if(newBc!=null){
						//更新债权状态为结束，债权存活期为到下一个还款日之前的间隔
						ca.setStatus(CreditAssignment.CREDIT_FINISH);
						creditAssignmentDao.update(ca);
					}
				}else{
					 oldBc=borrowCollectionDao.getCollectionByCredit(oldCl,i+"","0","");
				}
				if(oldBc!=null){
					logger.info("债权多次转让核心处理===");
		    		//债权转让出待收
			        double newCapital=0;
			        double newInterest=0;
			        double newRepaymentAccount=0;
				    double newCapitalTotal=0;
				    double newInterestTotal=0;
			        double newRepaymentAccountTotal=0;
				    for(CreditLog cl:creditLogList){
				    	BorrowCollection newBcn=borrowCollectionDao.getCollectionByCredit(cl,i+"","","");
				    	newCapital=newBcn.getCapital();
				    	newInterest=newBcn.getInterest();
				    	newRepaymentAccount=newBcn.getRepaymentAccount();
			    		newCapitalTotal+=newCapital;
			    		newInterestTotal+=newInterest;
			    		newRepaymentAccountTotal+=newRepaymentAccount;
				    }
				    //剩余未转出债权待收处理
				    double remainInterest=BigDecimalUtil.sub(BigDecimalUtil.sub(oldBc.getInterest(), newInterestTotal), newBc.getInterest());
				    double remainRepaymentAccount=BigDecimalUtil.sub(BigDecimalUtil.sub(oldBc.getRepaymentAccount(), newRepaymentAccountTotal),newBc.getRepaymentAccount());
				    if(remainInterest>0&&remainRepaymentAccount>0&&ca.getSoldAccount()>0){
				    	BorrowCollectionModel bcml=new BorrowCollectionModel();
						BorrowCollection bc=bcml.fillCollection(oldBc, BigDecimalUtil.sub(oldBc.getCapital(), newCapitalTotal),
			    		BigDecimalUtil.sub(oldBc.getInterest(), newInterestTotal), 
			    		BigDecimalUtil.sub(oldBc.getRepaymentAccount(), newRepaymentAccountTotal));
				        borrowCollectionDao.save(bc);
				    }else{
				    	logger.info("债权再转出，债权ID为"+ca.getId()+",剩余利息===="+remainInterest+",剩余待收金额=="+remainRepaymentAccount);
				    }
			    }
    		}
		}
	}
