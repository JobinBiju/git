package com.ddtkj.p2psys.credit.model;


/**
 * @描述：可转出债权model
 * @作者：yujunfei@ddtkj.com
 * @创建时间：2015年1月12日 
 */
public class CreditSwingOverMobileModel {
	
	private long id ;
	private int sellCopies      ;           // 成交份数
	private double  creditValue  ;               //公允价值(元)
    private double creditPrice   ;       //转让总价(元)
    private double haveAccount   ;         //  实际收入	
	private double fee ;             // 交易手续费
	private int buyCreditBalance;        // 盈亏  0 盈 1 亏
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public int getSellCopies() {
		return sellCopies;
	}
	public void setSellCopies(int sellCopies) {
		this.sellCopies = sellCopies;
	}
	public double getCreditValue() {
		return creditValue;
	}
	public void setCreditValue(double creditValue) {
		this.creditValue = creditValue;
	}
	public double getCreditPrice() {
		return creditPrice;
	}
	public void setCreditPrice(double creditPrice) {
		this.creditPrice = creditPrice;
	}
	public double getHaveAccount() {
		return haveAccount;
	}
	public void setHaveAccount(double haveAccount) {
		this.haveAccount = haveAccount;
	}
	public double getFee() {
		return fee;
	}
	public void setFee(double fee) {
		this.fee = fee;
	}
	public int getBuyCreditBalance() {
		return buyCreditBalance;
	}
	public void setBuyCreditBalance(int buyCreditBalance) {
		this.buyCreditBalance = buyCreditBalance;
	}
}
