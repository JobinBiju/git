
package com.ddtkj.p2psys.sms.exception;

import com.rongdu.common.exception.BussinessException;

/**
 * @描述: 短信通道异常类
 * @作者:shenguoping@ddtkj.com
 * @创建时间:  2015-2-12   
 */
public class SmsInterfaceException extends BussinessException {

	private static final long serialVersionUID = 2266349522705258744L;
	
	public SmsInterfaceException() {
		super();
	}
	
	public SmsInterfaceException(String message) {
		super(message);
	}

	public SmsInterfaceException(String message, int type) {
		super(message, type);
	}

	public SmsInterfaceException(String msg, RuntimeException ex) {
		super(msg, ex);
	}

	public SmsInterfaceException(String message, String url) {
		super(message, url);
	}
}
