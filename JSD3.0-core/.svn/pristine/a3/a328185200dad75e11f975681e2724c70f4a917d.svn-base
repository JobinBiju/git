package com.ddtkj.p2psys.core.cache;

/**
 * @描述:缓存运行异常
 * @作者:xinggenwang@ddtkj.com
 * @创建时间: 2014年12月26日
 */
public class MemcacheException extends RuntimeException {
	private static final long serialVersionUID = 7729562286541109048L;

	public MemcacheException(String msg, RuntimeException ex) {
		super(msg, ex);
	}

	public MemcacheException() {
		super();
	}

	public MemcacheException(String message) {
		super(message);
	}

}
