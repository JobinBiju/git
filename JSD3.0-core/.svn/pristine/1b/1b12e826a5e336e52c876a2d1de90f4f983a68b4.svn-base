
package com.ddtkj.p2psys.cardaward.exception;

import com.rongdu.common.exception.BussinessException;

/**
 * @描述: 购物卡转账自定义异常
 * @作者:shenguoping@ddtkj.com
 * @创建时间:  2015-3-31   
 */
public class CardAwardTransferException extends BussinessException {
	
	private static final long serialVersionUID = 5464982966891613954L;

	public CardAwardTransferException() {
		super();
	}

	public CardAwardTransferException(String message) {
		super(message);
	}

	public CardAwardTransferException(String message, int type) {
		super(message, type);
	}

	public CardAwardTransferException(String msg, RuntimeException ex) {
		super(msg, ex);
	}

	public CardAwardTransferException(String message, String url) {
		super(message, url);
	}

}
