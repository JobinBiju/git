package com.ddtkj.p2psys.credit.model;


/**
 * @描述：手机端
 * @作者：yujunfei@ddtkj.com
 * @创建时间：2015年1月12日 
 */
public class CreditSwingMobileModel {
	
	private long id  ; //记录id
	private long creditId  ;               // 债权ID
	private String borrowName ;  // 借款标题
	private double borrowApr  ;            //  年利率
    private String period   ;        //项目期数
	private int totalCopies ;              //总份数
	private int residueCopies ;                 //剩余份数
    private String repay_time ;      //应还日期 
    private double creditDis    ;      // 转让系数           
    private double creditValue  ;             //公允价值(元)
    private double creditPrice  ;           //转让总价(元)
	private String status ;  //  00 发布 01 初审通过   01不可撤回00可撤回
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public long getCreditId() {
		return creditId;
	}
	public void setCreditId(long creditId) {
		this.creditId = creditId;
	}
	public String getBorrowName() {
		return borrowName;
	}
	public void setBorrowName(String borrowName) {
		this.borrowName = borrowName;
	}
	public double getBorrowApr() {
		return borrowApr;
	}
	public void setBorrowApr(double borrowApr) {
		this.borrowApr = borrowApr;
	}
	public String getPeriod() {
		return period;
	}
	public void setPeriod(String period) {
		this.period = period;
	}
	public int getTotalCopies() {
		return totalCopies;
	}
	public void setTotalCopies(int totalCopies) {
		this.totalCopies = totalCopies;
	}
	public int getResidueCopies() {
		return residueCopies;
	}
	public void setResidueCopies(int residueCopies) {
		this.residueCopies = residueCopies;
	}
	public String getRepay_time() {
		return repay_time;
	}
	public void setRepay_time(String repay_time) {
		this.repay_time = repay_time;
	}
	public double getCreditDis() {
		return creditDis;
	}
	public void setCreditDis(double creditDis) {
		this.creditDis = creditDis;
	}
	public double getCreditValue() {
		return creditValue;
	}
	public void setCreditValue(double creditValue) {
		this.creditValue = creditValue;
	}
	public double getCreditPrice() {
		return creditPrice;
	}
	public void setCreditPrice(double creditPrice) {
		this.creditPrice = creditPrice;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}

}
