package com.rongdu.p2psys.core.exception;

import com.rongdu.common.exception.BussinessException;

/**
 * @描述:业务规则配置异常类
 * @作者:xinggenwang@ddtkj.com
 * @创建时间: 2015年1月10日
 */
public class RuleConfigException extends BussinessException {

	private static final long serialVersionUID = -7400559552805824955L;

	public RuleConfigException() {
		super();
	}

	public RuleConfigException(String message) {
		super(message);
	}

	public RuleConfigException(String message, int type) {
		super(message, type);
	}

	public RuleConfigException(String msg, RuntimeException ex) {
		super(msg, ex);
	}

	public RuleConfigException(String message, String url) {
		super(message, url);
	}

}
