
package com.ddtkj.p2psys.webpetcard.exception;

import com.rongdu.common.exception.BussinessException;

/**
 * @描述:
 * @作者:shenguoping@ddtkj.com
 * @创建时间:  2015-6-10   
 */
public class PetCardException extends BussinessException {
	

	private static final long serialVersionUID = 9063201719541152387L;

	public PetCardException() {
		super();
	}

	public PetCardException(String message) {
		super(message);
	}

	public PetCardException(String message, int type) {
		super(message, type);
	}

	public PetCardException(String msg, RuntimeException ex) {
		super(msg, ex);
	}

	public PetCardException(String message, String url) {
		super(message, url);
	}

}
