
package com.ddtkj.p2psys.redpacket.exception;

import com.rongdu.common.exception.BussinessException;

/**
 * @描述:
 * @作者:shenguoping@ddtkj.com
 * @创建时间:  2015-6-2   
 */
public class RpWeixinLogException extends BussinessException {

	private static final long serialVersionUID = 4300769218861374673L;
	
	public RpWeixinLogException() {
		super();
	}

	public RpWeixinLogException(String message) {
		super(message);
	}

	public RpWeixinLogException(String message, int type) {
		super(message, type);
	}

	public RpWeixinLogException(String msg, RuntimeException ex) {
		super(msg, ex);
	}

	public RpWeixinLogException(String message, String url) {
		super(message, url);
	}
	
}
