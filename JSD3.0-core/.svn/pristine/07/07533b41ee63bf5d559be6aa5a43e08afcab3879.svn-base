package com.ddtkj.p2psys.activity.exception;

import com.rongdu.common.exception.BussinessException;

/**
 * @描述：
 * @作者：wangchangwei@ddtkj.com
 * @创建时间：2015-7-23 
 */
public class ActivityGoodsException extends BussinessException {

	private static final long serialVersionUID = 4647356203113526812L;
	
	public ActivityGoodsException() {
		super();
	}

	public ActivityGoodsException(String message) {
		super(message);
	}

	public ActivityGoodsException(String message, int type) {
		super(message, type);
	}

	public ActivityGoodsException(String msg, RuntimeException ex) {
		super(msg, ex);
	}

	public ActivityGoodsException(String message, String url) {
		super(message, url);
	}
	
}
