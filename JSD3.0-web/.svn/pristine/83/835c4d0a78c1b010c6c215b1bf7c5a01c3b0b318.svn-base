@charset "utf-8";
*{margin:0;padding:0;list-style:none;font-style:normal;}
img{border:none;}
a{text-decoration:none;color:#333;}
body{min-width:1202px;font-size:14px;font-family:"微软雅黑";color:#333;position:relative;}
.zl_banner{width: 100%;height: 600px;background: url(../../images/subject/zlb.jpg) no-repeat center 0;}
.zl_con1{width: 544px;height: 456px;margin: 0 auto;text-align: center;}
.zl_con1 img{padding: 28px 0px 24px;margin: 0 auto;}
.zl_con1 p{color: #333333;font-size: 18px;line-height: 32px;}
.zl_con2{height: 234px;width: 100%;background: #73e0ff;position: relative;}
h3{color: #12adff;font-size: 30px;font-weight: normal;}
.zl_con2in{background:#fff;width: 643px;height: 393px;border: 1px solid #73e0ff;padding-left: 557px;margin: 0 auto;position: relative;top: 75px;}
.zl_con2in h3{padding-top: 88px;line-height: 66px;padding-left: 30px;}
.zl_con2in p{padding-left: 30px;color: #666666;font-size: 14px;line-height: 24px;width: 580px;}
.zl_con2in img{position: absolute;top: -30px;left: 30px;}
.zl_con3 h3{font-size: 34px;float: left;margin-top: 138px;}
.zl_con3{width: 1000px;margin: 235px auto 0;height: 329px;}
.zl_con3in{float: left;width: 775px;line-height: 24px;color: #666666;font-size: 14px;padding-top: 35px;padding-left: 75px;}
.zl_con3in p{margin-top: 20px;}
.zl_con4{text-align: center;}
.zl_con4 img{margin: 0 auto;margin-bottom: 66px;}
.zl_con5{width: 100%;height: 479px;background: #f3f3f3;}
.zl_con5in{width: 1202px;margin: 0 auto;text-align: center;}
.zl_con5in h3{line-height: 142px;}
.zl_con5in li{float: left;width: 400px;text-align: center;color: #666666;font-size: 14px;line-height: 40px;}
.zl_con5in li span{display: block;color: #333333;font-size: 22px;position: relative;width: 138px;margin: 0 auto 36px;text-align: center;}
.zl_con5in li span i{display: block;height: 2px;width: 50px;background: #666666;position: absolute;left: 42px;top: 40px;}
.zl_con6{width: 1202px;margin: 0 auto 30px;}
.zl_con6 h3{text-align: center;line-height: 120px;}
.zl_con6 ul{width: 1202px;height: auto;overflow: hidden;}
.zl_con6 ul li{float: left;padding-top: 2px;padding-bottom: 10px;}
.zl_con6 ul li dd{height: 36px;line-height: 36px;color: #666666;font-size: 14px;width: 574px;border-bottom: 1px dashed #d5d4d4;}
.zl_con6 ul li dd a{display: block;float: left;width: 430px;overflow: hidden; white-space: nowrap;  text-overflow: ellipsis;}
.zl_con6 ul li dd span{display: block;float: right;}