/* image-slider */
.y_slider{width: 558px;height: 175px;position: relative;margin-left: 40px;}
.y_sliderin{width: 558px;height: 175px;overflow: hidden;position: relative;}
.y_sliderpre{position: absolute;width: 25px;height: 25px;top: 79px;left: -60px;background: url(../../images/account/ps.png) no-repeat;cursor: pointer;}
.y_slidernext{position: absolute;width: 25px;height: 25px;top: 79px;right: -48px;background: url(../../images/account/ns.png) no-repeat;cursor: pointer;}
.y_sliderin ul{width: 2000px;height: 175px;position: absolute;left: 0;top: 0;display: none;}
.y_sliderin ul li{width: 174px;height: 175px;margin-right: 12px;float: left;overflow:hidden;}
.y_sliderin ul li img{width: 174px;height: 175px;}
.y_sliderin ul.showuls{display: block}