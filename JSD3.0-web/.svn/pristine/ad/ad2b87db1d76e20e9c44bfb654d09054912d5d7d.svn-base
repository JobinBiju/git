@charset "utf-8";
body,p,h1,h2,h3,h4,h5,h6,form,input,ul,li,dd,dl,textarea{
  padding:0;margin:0;
  list-style-type:none;
}
a{
  text-decoration:none;
  font-size:14px;
  color:#858797;
}
img{
  border:0;
}
@font-face{font-family:HelveticaNeueLt;
src:url(../bg/font/helveticaneuelt.eot);
src:url(../bg/font/helveticaneuelt.eot?#iefix)format('embedded-opentype'),
url(../bg/font/helveticaneuelt.woff)format('woff'),
url(../bg/font/helveticaneuelt.ttf)format('truetype'),
url(../bg/font/helveticaneuelt.svg#HelveticaNeueLt)format('svg');font-weight:400;font-style:normal;}