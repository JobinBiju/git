.modal-header {padding: 9px 15px;border-bottom: 1px solid #eee;}
.modal.fade {-webkit-transition: opacity .3s linear,top .3s ease-out;-moz-transition: opacity .3s linear,top .3s ease-out;-ms-transition: opacity .3s linear,top .3s ease-out;-o-transition: opacity .3s linear,top .3s ease-out;transition: opacity .3s linear,top .3s ease-out;top: -25%;}
.close {float: right;font-size: 20px;font-weight: 700;line-height: 20px;color: #000;text-shadow: 0 1px 0 #fff;opacity: .2;filter: alpha(opacity=20);}
.modal-header h3 ,.modal-body h4{margin: 0;line-height: 30px;}
.modal-body {position: relative;overflow-y: auto;max-height: 400px;padding: 15px;}
#Contract{opacity:1;background:#fff;z-index:2000;position: absolute;width: 800px;font-size:14px;}
#Contract ol{margin:0;}
#Contract ol,.modal-body p{padding-left: 20px;}
#Contract ol p{padding:0;}
#Contract ol li{line-height:20px;}
.modal-footer {padding: 14px 15px 15px;margin-bottom: 0;text-align: right;background-color: #f5f5f5;border-top: 1px solid #ddd;-webkit-border-radius: 0 0 6px 6px;-moz-border-radius: 0 0 6px 6px;-ms-border-radius: 0 0 6px 6px;-o-border-radius: 0 0 6px 6px;border-radius: 0 0 6px 6px;-webkit-box-shadow: inset 0 1px 0 #fff;-moz-box-shadow: inset 0 1px 0 #fff;-ms-box-shadow: inset 0 1px 0 #fff;-o-box-shadow: inset 0 1px 0 #fff;box-shadow: inset 0 1px 0 #fff;}
.modal.fade.in {top: 10%;}
.modal-backdrop {position: fixed;top: 0;right: 0;bottom: 0;left: 0;z-index: 1040;background-color: #000;}
.modal-backdrop, .modal-backdrop.fade.in {opacity: 0.8;filter: alpha(opacity=80);}
.fade {opacity: 0;filter: alpha(opacity=0);-webkit-transition: opacity .15s linear;-moz-transition: opacity .15s linear;-ms-transition: opacity .15s linear;-o-transition: opacity .15s linear;transition: opacity .15s linear;}
.btn-primary {border-style:none;background-color: #06b5ff;background-image: none;background-repeat: repeat-x;border: 0;filter: initial;-webkit-box-shadow: none;-moz-box-shadow: none;box-shadow: none;width: 60px;height: 30px;border-radius: 4px;color: #fff;font-family: microsoft Yahei;}