@charset "utf-8";
*{margin:0;padding:0;list-style:none;font-style:normal;}
img{border:none;}
a{text-decoration:none;color:#333;}
body{min-width:1002px;font-size:14px;font-family:"微软雅黑";color:#333;position:relative;}
.cloud_content{height:1797px;background:url(../../images/cardTransfer/general_bj2.jpg) no-repeat center 0;}
.text_content{width:1002px;margin:0 auto;}
.notice{height:112px;padding-top:787px;padding-left:104px;line-height:28px;font-size: 15px;}
.before_logo{width:449px;margin-left: 381px;height:69px;background:url(../../images/cardTransfer/before_logo2.jpg) no-repeat 0;}
.login_before{margin-top:90px;height:327px;}
.login_after{margin-top:178px;height:239px;}
.all_text{width:456px;margin-left: 371px;}
.all_buy{margin-left: 5px;margin-top: 20px;width:548px;height:121px;background:url(../../images/cardTransfer/cloud_logo23.jpg) no-repeat;}
.login_text{float: left;width:296px;font-size:15px;line-height: 28px;margin-left: 15px;}
.login_btns{float: left;display:block;margin-top: 15px;width:108px;margin-left:80px;text-align:center;height:28px;line-height:28px;background:#cb2c2c;color:#fff;font-size:18px;}
.active_rule{height:328px;width:776px;margin:0px auto;line-height:32px;font-size:15px;color:#222;font-weight:600;}
.active_rule span{font-size: 16px;padding-left: 25px;display: block;}
.JDgoatyear4 {
width: 730px;
margin: 0 auto;
height: 1136px;
margin-top: 298px;

}
.JDgoatyear4 ul {
width: 359px;
float: left;
height: 1105px;
margin-bottom: 28px;
}
.JDgoatyear4 ul li {
margin-bottom: 1.1px;
width: 100%;
height: 21px;
line-height: 21px;
text-align: center;
color:#333;
font-size: 15px;
