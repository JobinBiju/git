}
.JDgoatyear4 ul li span.JDgoatyear4span1 {
width: 40px;
}
.JDgoatyear4 ul {
width: 362px;
float: left;
height: 1105px;
margin-bottom: 28px;
}
.JDgoatyear4 ul.JDgoatyear4ul2 {
margin-left: 1px;
}
.JDgoatyear4 ul li span.JDgoatyear4span2 {
width: 120px;
}
.JDgoatyear4 ul li span.JDgoatyear4span3 {
width: 92px;
}
.JDgoatyear4 ul li span.JDgoatyear4span4 {
