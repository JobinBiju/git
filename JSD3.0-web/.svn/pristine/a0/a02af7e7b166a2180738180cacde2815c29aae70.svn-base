/**********************************************b_recharge.html账户充值页面*******************************************/
.c_recharge{margin-top:20px;width:742px;border:1px solid #DDDDDD;padding: 18px 26px;}
.c_recharge dd{float:left;width:180px;font-size:14px;color:#333;margin-right:20px;}
