.c_recharge_detail dl dd i{display:block;width:35px;height:20px;background:url(../../images/b_money/detail.png) no-repeat -1px;}
.c_left_more,.c_recharge_more{display:none;}
.c_page{float:right;margin-top: 20px;}
.c_page dd{cursor:pointer;font-size:12px;color:#000;margin-right:8px;float:left;text-align:center;width:18px;height:18px;line-height:18px;border:1px solid #CCCCCC;}
.c_page .c_page_btn{font-family:Simsun;text-align:center;width:28px;}
.c_page_this{background:#eeeeee;}
.c_nodata{width:83px;height:76px;margin:80px auto;background:url("../../images/b_money/none_data.png");} 
