/*css sprite*/
#banner_wrap .banner { position:absolute; left:0; top:0; width:100%; height:450px; min-width:1190px; }
#banner_wrap .banner_con { width:100%; height:450px; }
#banner_wrap .banner_con a{display: block;height: 450px;width:1190px;}
#banner_wrap .flexslider{ position:relative; height:450px; overflow:hidden; background:url(../bg/banner_loading.gif) 50% no-repeat; }
#banner_wrap .slides{ position:relative; z-index:1; }
#banner_wrap .slides li{ height:450px; }
#banner_wrap .flex-control-nav{ position:absolute; bottom:30px; z-index:2; width:100%; text-align:center; }
#banner_wrap .flex-control-nav li{ display:inline-block; width:14px; height:14px; margin:0 5px; *display:inline; zoom:1; }
#banner_wrap .flex-control-nav a{ display:inline-block; width:14px; height:14px; line-height:40px; overflow:hidden; background:url(../bg/indexSprite.png) 0 -1519px no-repeat; cursor:pointer; }
#banner_wrap .flex-control-nav .flex-active{ background:url(../bg/indexSprite.png) 0 0 no-repeat; }
#banner_wrap .flex-direction-nav{ position:absolute; z-index:3; width:100%; top:186px; }
#banner_wrap .flex-direction-nav li a{ display:block; width:58px; height:58px; overflow:hidden; cursor:pointer; position:absolute; background:url(../bg/indexSprite.png) top left no-repeat; }
#banner_wrap .flex-direction-nav li a.flex-prev{ left:20px; background-position: 0 -1656px; }
#banner_wrap .flex-direction-nav li a.flex-prev:hover{ background-position: 0 -1764px; }
#banner_wrap .flex-direction-nav li a.flex-next{ right:20px; background-position: 0 -1303px; }
#banner_wrap .flex-direction-nav li a.flex-next:hover{ background-position: 0 -1411px; }

/*登录*/
.login_box{ position:relative; margin:0 auto; width:1190px; z-index:9; }
.login_box .login_bg{ position:absolute; right:0px; top:30px; width:300px; height:380px; background-color:#000; opacity:.4; filter:alpha(opacity=40); }
.login_box dl { position:absolute; right:33px; top:30px; width:235px; height:380px; }
.login_box dd,.login_box dt { width:235px; margin:0 auto; }
.login_box dt{ padding-top:20px; line-height:52px; }
.login_box dt span{ font-size:20px; float:left; color:#fff; font-weight:bold; }
.login_box dt font { float:right; color:#fff; }
.login_box dt font a { color:#66b128; }
.login_box dd{ line-height:40px; }
.login_box .username, .login_box .password{ display:block; color:#fff; margin-bottom:11px; width:233px; height:28px; line-height:28px; padding:5px 0; border: 1px solid #84808d; text-indent:42px; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius: 5px; }
.login_box .username{ background:url(../bg/indexSprite.png) #666 -145px -167px no-repeat; opacity:.5; filter:alpha(opacity=50); }
.login_box .password{ background:url(../bg/indexSprite.png) #666 12px -1576px no-repeat; opacity:.5; filter:alpha(opacity=50); }
.login_box .code{ width:142px; height:28px; line-height:28px; padding:5px 0; color:#ccc; background:url(../bg/indexSprite.png) -145px -236px no-repeat; border: 1px solid #84808d; text-indent:42px; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius: 5px; background-color:#666; opacity:.5; filter:alpha(opacity=50); } 
.login_box .valicode_img{ display:inline-block; height:36px; width:78px; margin-left:6px; padding:0; border:1px solid #e4e4e4; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius: 5px; vertical-align: middle; cursor:pointer; }
.login_box label { display: inline; color:#fff; }
.login_box label input{ float:left; margin:14px 2px 0 0; }
.login_box .forget { float:right; color:#fff; }
.login_box .errorTips{ display:block; height:36px; margin-bottom:10px; }
.login_box .errorTips em{ display:block; padding-left:10px; line-height:34px; border:1px solid #ffd8d9; background-color:#fcebec; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius: 5px; }
.login_box .errorTips em i{ color:#c83e41; margin-right:10px; }
.login_box .errorTips em b{ font-weight:normal; }
.login_box .errorTips em b a{color:#c83e41;margin-left:5px;font-weight:bold;}
.login_box .login_btn { width:235px; line-height:40px; height:40px; color:#fff; text-align:center; background-color:#70bc31; border:none; border-bottom:2px solid #66b128; cursor:pointer; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius: 5px; font-size:16px; }
/* 
.login_box .error { border: 1px solid #f00; background-position: 0 -469px; width:220px; height:32px; } */


