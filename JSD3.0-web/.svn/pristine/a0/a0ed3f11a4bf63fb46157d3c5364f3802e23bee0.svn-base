@charset "utf-8";
/* CSS Document */
*html{ background-image:url(about:blank); background-attachment:fixed; }
/*-S-首页 头尾 css样式*/
#header{ width:100%; }
#header_info{ width:100%; min-width: 1190px; background:#333; color:#999; }
.sprite{ background:url(../bg/indexSprite.png) top left no-repeat; }
.header_top { margin:0 auto; width:1190px; font-size: 12px; }
.header_top .float_left,.header_top .float_right{ line-height:40px; }
.header_top .float_left span{ display:inline-block; }
.header_top .home{ background-position: 0 -926px; padding-left:25px; }
.header_top .tel{padding-left:36px;}
.header_top .tel em{font-size:20px;vertical-align: middle;}
.header_top .attention{ padding-left:42px; }
.header_top .attention em{ position:relative; display:inline-block; height:24px; width:24px; margin-left:6px; vertical-align: middle; text-indent:-999999px; cursor:pointer; }
.header_top .attention em .topTipsWrap{ display:none; position:absolute; line-height:normal; z-index:4; width:120px; top:18px; padding-top:20px; left:-14px; font-family:"Microsoft YaHei","arial"; text-indent:0; }
.header_top .attention em .topTips{ position:absolute; top:13px; width:98px; padding:14px 10px; font-size:14px; background-color:#f5f5f5; border:1px solid #e6e6e6; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius: 5px; }
.header_top .attention em .topTips img{width: 160px;height: 160px;}
.header_top .attention em .topTipsWrap i{ position:absolute; top:7px; left:20px; display:inline-block; height:7px; width:11px; background:url(../bg/hoveTips.png) no-repeat; z-index:10; }
.header_top .attention em .topTips h2{ padding-bottom:12px; color:#333; }
.header_top .attention em .topTips a{ color:#4971b6; }
.header_top .attention em .topTips span{ line-height:26px; color:#b7322c; }
.header_top .attention em .topTips span b{ font-weight:normal; font-size:19px; }
.header_top .attention em.i_weibo{ background-position: -154px -462px; }
.header_top .attention em.i_weixin{ background-position: -154px -314px; }
.header_top .attention em.i_qq{ background-position: 0 -1872px; }
.header_top .attention em.i_weibo:hover{ background-position: -154px -536px; }
.header_top .attention em.i_weixin:hover{ background-position: -154px -388px; }
.header_top .attention em.i_qq:hover{ background-position: 0 -1946px; }
.header_top .attention em:hover .topTipsWrap{ display:block; }
.header_top .float_right a{ display:inline-block; *zoom:1; *display:inline;}
.header_top .float_right a:link,.header_top .float_right a:visited{ color:#999; }
.header_top .float_right a:hover{ color:#fff; text-decoration:none; }
.header_top .float_right .noborder{ background:none; padding-right:0; }
.header_top .loginStatus a{margin-left:2px;}
.header_top .loginStatus a.guide{border:1px solid #ccc;color:#ccc;padding:0 5px;line-height:23px;border-radius:5px;}
.header_top .loginStatus a.guide:hover{border:1px solid #fff;color:#fff;}
.header_con_wrapper { width:100%; background:#fff; border-bottom:1px solid #dcdcdc; min-width:1190px; padding:23px 0; }
.header_top a{ color:#eee; }
#header .logo { float:left; }
#header .logo img{ display:block; }
.memberLogo { float:left; }
.memberLogo a{display:block;width:365px;height:67px;background:url(../bg/memberLogo.png) no-repeat;position:relative;}
.memberLogo a span{position:absolute;right:12px;top:22px;font-size:18px;color:#aaa;}
.header_con{ width:1190px; margin:0 auto; }

#nav { float:right; overflow:hidden; padding-top:17px; width: 700px; text-align: right; }
#nav a{ display: inline-block; width: 90px; height: 32px; line-height: 32px; text-align: center; -moz-border-radius:5px; -webkit-border-radius:5px; border-radius: 5px; font-size: 16px; color: #333; }
#nav a.hover,#nav a:hover{ background: #315a8b; text-decoration: none; color: #fff; }
#nav .hover{ background: #315a8b; text-decoration: none; color: #fff; }
#nav .invest_link{ background: #dddddd; text-decoration: none; color: #666666;}
#nav .borrow_link{ background: #dddddd; text-decoration: none; color: #666666;margin-left:16px;}
#nav .member_nav{border-right:1px solid #e8e8e8;margin-right:30px;}
#nav .member_nav a{margin-right:13px;}
#nav .myAccount{display:inline-block;height:32px;margin-left:10px;padding-left:20px;border-left:1px solid #e8e8e8;}
#nav .myAccount a{background: #dddddd; text-decoration: none; color: #666666;}
#nav .myAccount a:hover{background: #315a8b; text-decoration: none; color: #fff;}

#footer{ width:100%; background-color: #333; height: 194px; min-width: 1190px; }
.footer_content{ margin: 0 auto; width:1190px; font-size: 14px; color:#aaa;}
.footer_content .footerLink{line-height:40px;padding:20px 0 0 0;}
.footer_content .footerLink a{margin-right:16px;}
.footer_content .footerLink a:link,.footer_content .footerLink a:visited{color:#aaa;}
.footer_content .footerLink a:hover{color:#fff;}
.footer_content .float_left p{line-height:35px;}
.footer_content .float_left p a{color:#aaa;}
.footer_content .float_left p em{padding-left:14px;}
.footer_content .footerPic{padding-top:10px;}
.footer_content .footerPic a{display: inline-block; margin: 0 25px 0 0; background: url(../bg/footer_basebg.png);vertical-align: middle;}
.footer_content .footerPic .trust{background-position: 0 0; width: 42px; height: 42px; }
.footer_content .footerPic .credible{background-position: -92px 0; width: 102px; height: 38px;}
.footer_content .footerPic .sincerity{background-position: -244px 0; width: 95px; height: 34px;}
.footer_content .footerPic .norton{background-position: -389px 0; width: 96px; height: 53px}
.customerservice{height:140px;border-right:2px solid #555;text-align:right;padding-right:22px;margin:30px 20px 0 0;line-height:45px;}
.customerservice h3,.customerservice em{font-size:18px;}
.customerservice span{display:block;font-size:32px;}
.footerKf{width:114px;float:right;padding:30px 14px 0 0;}
.footerKf em{position:relative; display:inline-block; background: url(../bg/footer_kf.png);margin-left:6px; vertical-align: middle; text-indent:-999999px; cursor:pointer; }
.footerKf a{display: block;}
.footerKf em.weibo{background-position: 0 0; width: 100px; height: 30px;margin-top:5px;}
.footerKf em.weibo a{display: block;width: 100px; height: 30px;}
.footerKf em.weibo:hover{background-position: 0 -80px;}
.footerKf em.weixin{background-position: 0 -160px; width: 92px; height: 31px;margin-top:14px;}
.footerKf em.weixin:hover{background-position: 0 -240px;}
.footerKf em.kf{background-position: 0 -322px; width: 114px; height: 30px;margin-top:14px;}
.footerKf em.kf a{width: 114px; height: 30px;}
.footerKf em.kf:hover{background-position: 0 -402px;}
.footerKf em .bottomTipsWrap{display:none; position:absolute;height:160px; width:160px;padding:10px;background-color:#fff; line-height:normal; z-index:4; top:-188px;left:-14px; font-family:"Microsoft YaHei","arial"; text-indent:0;-moz-border-radius:6px; -webkit-border-radius:6px; border-radius: 6px;}
.footerKf em .bottomTipsWrap i{ position:absolute; bottom:-10px; left:50px; display:inline-block; height:10px; width:20px; background:url(../bg/bottomHoveTips.png) no-repeat; z-index:10; }
.footerKf em:hover .bottomTipsWrap{ display:block; }
.footerKf em .bottomTipsWrap img{width: 160px;height: 160px;}

