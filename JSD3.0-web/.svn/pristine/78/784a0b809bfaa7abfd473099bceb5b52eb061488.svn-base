@charset "utf-8";
*{margin:0;padding:0;font-family:microsoft Yahei;list-style:none;}
img{border:none;}
a{text-decoration:none;}
body{min-width: 1200px;margin: 0 auto;}
/*banner*/
.dw-banner{width: 100%;height: 941px;background: url(../../images/dw-bannerbj.jpg) no-repeat center 0px;position: relative;}
.dw-banner-bo{position: absolute;bottom: 0px;left: 0px;height: 193px;background: url(../../images/dw-banner2.png) no-repeat center 0px;width: 100%;}
.dw-bannerin{width: 1200px;margin: 0 auto;position: relative;height: 941px;}
.dw-boat{position: absolute;left: 0px;bottom: 40px;-webkit-animation: swing 4s linear infinite alternate;-moz-animation: swing 4s linear infinite alternate;animation: swing 4s linear infinite alternate;}
.dw-return{display:block;width: 130px;padding-left:20px;height: 38px;background: url(../../images/dw-return.png) no-repeat 0px 3px;color: #58595b;font-size: 24px;position: relative;top: 77px;left: 13px;}
/*列表*/
.dw-con1{width: 100%;background: #f0fcff;height: 652px;background: #f0fcff url(../../images/con-bj.png) no-repeat center 0px;}
.dw-list{width: 826px;margin: 0 auto;padding-top: 295px;}
.dw-list-in{height: 237px;width: 826px;overflow: hidden;border-bottom: 1px dashed #4b4b4b;}
.dw-list ul{position: relative;}
.dw-list ul li{border-top: 1px dashed #4b4b4b;height: 106px;width: 826px;padding-top: 13px;}
.dw-list ul li dd{float: left;color: #4b4b4b;font-size: 16px;line-height: 30px;text-align: center;}
.dw-list ul li dd.dw-list-dd1{width: 194px;}
.dw-list ul li dd.dw-list-dd2{width: 200px;}
.dw-list ul li dd.dw-list-dd3{width: 120px;}
.dw-list ul li dd.dw-list-dd4{width: 200px;}
.dw-list ul li dd.dw-list-dd5{width: 110px;}
.dw-now-click{display: block;color: #000000;font-size: 26px;border: 2px solid #2a2327;width: 184px;height: 44px;line-height: 44px;text-align: center;margin: 28px auto 27px;}
.dw-now-click1{margin-top: 54px;}
.dw-now-click2{border: 2px solid #fff;color: #fff;}
.dw-now-click:hover{border: 2px solid #ff1919;background: #ff1919;color: #fff;}
.dw-con2{width: 100%;height: 277px;background: url(../../images/con-bj1.png) no-repeat center 0px;}
/*最后模块*/
.dw-con-last{width: 100%;height: 1071px;background: #008d72 url(../../images/con-bj2.png) no-repeat center 0px;}
/*图片轮转*/
#box{position:relative;width:962px;height:482px;margin:0 auto;padding-top: 35px;}
#box ul{position:relative;width:694px;height:490px;position: absolute;left: 122px;}
#box li{position:absolute;list-style:none;background:#000;width:0;height:0;top:146px;left:322px;z-index:0;cursor:pointer;overflow:hidden;border-radius:20px;}
#box li img{width:100%;height:100%;vertical-align:top;}
#box .prev, #box .next{position:absolute;top:212px;width:17px;height:56px;overflow:hidden;text-indent:-999px;cursor:pointer;}
#box .prev{left:0px;background:url(../../images/dw-btl.png) no-repeat;}
#box .next{right:0px;background:url(../../images/dw-btr.png) no-repeat;}
/*玩法*/
.dw-box{width: 870px;margin: 0 auto;color: #ffffff;}
.dw-box h3{font-size: 30px;line-height: 60px;text-align: center;}
.dw-box p{font-size: 16px;line-height: 40px;}
.dw-box span{color: #fef26f;}
.dw-box-in{display: none;}
.dw-box-in1{display: block;}
.dw-box-p{width: 894px;height: 189px;margin: 14px auto 0;background: #a6ffee;font-size: 17px;color: #2f2f2f;line-height: 40px;padding-left: 34px;padding-top: 22px;}
.dw-box-p span{padding-left: 268px;}
@-webkit-keyframes swing{20%{-webkit-transform:rotate(2deg)}40%{-webkit-transform:rotate(-2deg)}60%{-webkit-transform:rotate(1deg)}80%{-webkit-transform:rotate(-1deg)}100%{-webkit-transform:rotate(0deg)}}
@-moz-keyframes swing{20%{-moz-transform:rotate(2deg)}40%{-moz-transform:rotate(-2deg)}60%{-moz-transform:rotate(1deg)}80%{-moz-transform:rotate(-1deg)}100%{-moz-transform:rotate(0deg)}}
@keyframes swing{20%{transform:rotate(2deg)}40%{transform:rotate(-2deg)}60%{transform:rotate(1deg)}80%{transform:rotate(-1deg)}100%{transform:rotate(0deg)}}

