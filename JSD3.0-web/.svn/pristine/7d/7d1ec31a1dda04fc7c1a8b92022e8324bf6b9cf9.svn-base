<dl class="c_data_title">
	<dd class="c_data_titledd90">类型名</dd>
	<dd class="c_data_titledd90">操作积分</dd>
	<dd class="c_data_titledd90">剩余有效积分</dd>
	<dd class="c_data_titledd90">剩余冻结积分</dd>
	<dd style="width:140px;">记录时间（分秒）</dd>
	<dd class="c_data_titledd250">备注信息</dd>
	<div class="c_clear"></div>
</dl>
{{#each data.list}}
<dl>
	<dd class="c_data_titledd90">{{scoreTypeName}}</dd>
	<dd class="c_data_titledd90">{{score}}</dd>
	<dd class="c_data_titledd90">{{validScore}}</dd>
	<dd class="c_data_titledd90">{{freezeScore}}</dd>
	<dd style="width:140px;">{{transFormatDate addTime}}</dd>
	<dd class="c_data_titledd250">{{{remark}}}</dd>
	<div class="c_clear"></div>
</dl>							
{{/each}}	
