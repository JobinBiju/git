<script>
 $(function(){
 	$(".nav dd a").removeClass("c_nav_this");
	$(".nav dd a").eq(2).addClass("c_nav_this");
 	var num=0;
 	$(".left-btn-5-div").click(function(){
 			if (!($(".Newbie-guide-box5 dl").is(":animated"))) {
 		var lefts=parseInt($(".Newbie-guide-box5 dl").css("marginLeft").slice(0,-2));
 		if(num>0){
 			num--;
 		}else{
 			num=3;
 		}
 		if(lefts<0){
 		$(".Newbie-guide-box5 dl").animate({marginLeft:lefts+820},600);
 		}else if(lefts==0){
 			$(".Newbie-guide-box5 dl").css("marginLeft","-3280px");
 			$(".Newbie-guide-box5 dl").animate({marginLeft:-3280+820},600);
			
 		}
 		$($(".Newbie-guide-box5 ul li")[num]).addClass("ul5likk").siblings().removeClass("ul5likk");	
 		}
 	})
 	$(".right-btn-5-div").click(function(){
 			if (!($(".Newbie-guide-box5 dl").is(":animated"))) {
 		var lefts=parseInt($(".Newbie-guide-box5 dl").css("marginLeft").slice(0,-2));
 		if(num<3){
 			num++;
 		}else{
 			num=0;
 		}
 		if(lefts>-5740){
 		$(".Newbie-guide-box5 dl").animate({marginLeft:lefts-820},600);
 		}else if(lefts==-5740){
 			$(".Newbie-guide-box5 dl").css("marginLeft","-2460px");
 			$(".Newbie-guide-box5 dl").animate({marginLeft:-3280},600);
			
 		}
 		$($(".Newbie-guide-box5 ul li")[num]).addClass("ul5likk").siblings().removeClass("ul5likk");	
 		}
 	})
 	$(".Newbie-guide-box5 ul li").click(function(){
 	if (!($(".Newbie-guide-box5 dl").is(":animated"))) {
 		var lefts=parseInt($(".Newbie-guide-box5 dl").css("marginLeft").slice(0,-2));
 		$(this).addClass("ul5likk").siblings().removeClass("ul5likk");
 			var index=$(this).index(".Newbie-guide-box5 ul li");
 		if(lefts<=-3280){
 		$(".Newbie-guide-box5 dl").animate({marginLeft:-820*(index+4)},600);
 		num=index+4;
 		}else{
 		$(".Newbie-guide-box5 dl").animate({marginLeft:-820*index},600);
		num=index;
 		}
 	}
 	})
 })
</script>
