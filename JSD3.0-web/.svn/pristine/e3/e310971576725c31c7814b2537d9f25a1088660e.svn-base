@charset "utf-8";
@import url(comm.css);
body{min-width:1124px;background: #fff; _width:expression(document.documentElement.clientWidth>1124?document.documentElement.clientWidth:1124);font-family:"Microsoft YaHei";font-size:14px;}
em,h1,h2,h3,h4{font-style: normal;font-weight: normal;}
.clear{clear:both;}
.w_banner{width:100%;height:410px;background:url(../../images/subject/icon_02.jpg) no-repeat;position: relative;}
.login-regist{width:292px;height:352px;background:#fff;border-radius:10px;-o-border-radius:10px;-webkit-border-radius:10px;-moz-border-radius:10px;-ms-border-radius:10px;
box-shadow: 3px 3px 7px #555;-o-box-shadow: 3px 3px 7px #555;-webkit-box-shadow: 3px 3px 7px #555;-moz-box-shadow: 3px 3px 7px #555;-ms-box-shadow: 3px 3px 7px #555;
position: absolute;right:50%;top:29px;margin-right:-440px;}
.login-regist h3{text-align: center;font-size:19px;color:#333;line-height:50px;}
.w_form{width:224px;margin:0px auto;}
.w_form .w_re_lab{width: 224px;margin: 0 auto 10px;position: relative;}
.w_form .w_re_lab input{padding-left:43px;border:1px solid #999;border-radius:2px;-o-border-radius:2px;-webkit-border-radius:2px;-moz-border-radius:2px;-ms-border-radius:2px;outline: none;width: 181px;height: 34px;}
.w_form .w_re_usual span{position: absolute;top: 8px;left: 9px;display: block;width: 28px;height: 22px;border-right: 1px solid #999;}
.w_form .w_re_usual span.w_phone_icon{background:url(../../images/subject/icons_one_03.png) no-repeat center center;}
.w_form .w_re_usual span.w_user_icon{background:url(../../images/subject/icons_one_07.png) no-repeat center center;}
.w_form .w_re_usual span.w_pass_icon{background:url(../../images/subject/icons_one_10.png) no-repeat center center;}
.w_form .w_re_code input{width: 101px;padding-left: 10px;}
.w_form .w_re_code span{position:absolute;left:118px;top:0;display: block;width: 102px;height: 34px;border:1px solid #999;border-radius:2px;-o-border-radius:2px;-webkit-border-radius:2px;-moz-border-radius:2px;-ms-border-radius:2px;}
.w_form .w_re_code span img{width: 102px;height: 32px;}

.w_add_link{display:block;margin-top:20px;background:url(../../images/subject/icons_one_14.png) no-repeat center center;font-size:17px;color:#fff;text-align: center;width:223px;height:39px;line-height:36px;}
.w_content{width:1000px;margin:0px auto;}
.w_first_content{margin-top:10px;}
.w_red{float:left;width:227px;height:322px;position: relative;}
.w_red img{width:250px;height:322px;position: absolute;left:-23px;top:0px;}
.w_red a{display:block;width:106px;height:35px;background:url(../../images/subject/icons_22.png) no-repeat center center;position: absolute;left:72px;bottom:40px;z-index: 1;}
.w_ul_list{float:left;margin-left:30px;}
.w_ul_list li{float:left;width:189px;margin:20px 28px 0px;text-align: center;}
.w_ul_list li img{width:137px;height:137px;}
.w_ul_list li h4{font-size:18px;color:#626262;margin:12px 0px;}
.w_ul_list li p{font-size:13px;color:#626262;line-height:24px;}
.w_ul_list li a{display:block;width:177px;height:41px;border-radius:6px;-o-border-radius:6px;-webkit-border-radius:6px;-moz-border-radius:6px;-ms-border-radius:6px;background:#8cc81a;color:#fff;line-height:41px;font-size:18px;margin-top:20px;}
.w_ul_list li a:hover{background:#4ab9e4;}

.w_product_content{margin-top:40px;}
.w_product_content h3{color:#fff;font-size:25px;text-align: center;background:url(../../images/subject/icon_19.png) no-repeat center center;height:64px;line-height:44px;}
.w_product_details{margin-top:10px;overflow:hidden;}
.w_product_details dd{float:left;margin-left:58px;margin-bottom:25px;border:2px solid #4ab9e4;width:424px;height:214px;border-radius:4px;-o-border-radius:4px;-webkit-border-radius:4px;-moz-border-radius:4px;-ms-border-radius:4px;position: relative;}
.w_product_details .w_add_margin{margin-right:28px;}
.w_product_icon{position: relative;left:-58px;top:45px;width:123px;float:left;}
.w_product_icon img{width:123px;height:123px;}
.w_product_right{width:284px;margin-left:114px;}
.w_product_right h4{font-size:18px;color:#555;font-weigth:bold;text-align: center;margin:20px 0px;}
.w_product_right p{font-size:14px;color:#666;line-height:24px;}
.w_product_right a{display:block;width:245px;height:46px;font-size:16px;color:#fffefe;background:#4ab9e4;line-height:46px;text-align: center;border-radius:4px;-o-border-radius:4px;-webkit-border-radius:4px;-moz-border-radius:4px;-ms-border-radius:4px;margin-top:15px;}
.w_product_right a:hover{background:#039ee8;}

.w_advantage{margin-top:20px;}
.w_advantage h3{color:#fff;font-size:25px;text-align: center;background:url(../../images/subject/icon_38.png) no-repeat center center;height:64px;line-height:44px;}
.w_advantage_details{width:934px;margin:30px 33px;}
.w_advantage_details li{float:left;width:310px;text-align:center;border:1px solid #c9c9c9;border-right:0px;}
.w_advantage_details .w_center_right{border:1px solid #c9c9c9;border-left:0px;}
.w_advantage_details li h2{font-size:25px;color:#161616;line-height:72px;border-bottom: 1px dashed #c9c9c9;height:72px;}
.w_advantage_details li h2 img{margin-top:10px;}
.w_earnings{height:115px;border-bottom: 1px dashed #c9c9c9;}
.w_earnings span{display:block;float:left;width:155px;text-align: center;color:#7f7f7e;font-size:20px;line-height:30px;margin-top:30px;}
.w_earnings span i{font-style:normal;display:block;color:#0796da;font-size:24px;line-height:34px;}
.w_advantage_details li p{font-size:18px;color:#555555;line-height:70px;border-bottom: 1px dashed #c9c9c9;}
.w_advantage_details .w_center_con{border:1px solid #ef7e00;position: relative;}
.w_advantage_details li .w_last{border:none;}
.w_add_header{width:312px;height:20px;background:#ef7e00;position: absolute;top:-20px;left:-1px;}
.w_center_con .w_earnings span i{color:#ef7e00;}
.w_advantage_details .w_center_con p{background:url(../../images/subject/login_07.png) no-repeat 60px center;}

.footer_banner{background:url(../../images/subject/icon_41.jpg) no-repeat;height:381px;}
.footer_banner_in{width:1000px;margin:0px auto;position: relative;height:381px;}
.footer_banner_in a{display:block;text-align: center;width:234px;height:60px;background:url(../../images/subject/icons_41.png) no-repeat;color:#fff;font-size:28px;line-height:50px;position:absolute;bottom:80px;left:50%;margin-left:-167px;}
.w_data p{color:#ef7e00;text-align: center;}
.w_data{position: absolute;}
.w_data_one{width:250px;left:-25px;top:72px;}
.w_data_two{width:200px;left:250px;top:84px;}
.w_data_three{width:250px;left:510px;top:65px;}
.w_data_four{width:226px;right:-38px;top:72px;}
.w_data_one p{font-size:28px;line-height:40px;}
.w_data_one .titles{font-size:22px;line-height:30px;}
.w_data_two p{font-size:28px;line-height:40px;}
.w_data_two .titles{font-size:20px;line-height:30px;}
.w_data_three p{font-size:26px;line-height:40px;}
.w_data_three .titles{font-size:21px;line-height:30px;}
.w_data_four .w-num{font-size:34px;line-height:46px;width:200px;margin-left:26px;}
.w_data_four .titles{font-size:22px;line-height:30px;}

/*右侧*/
.g-toolbar {
position: fixed;
top: 60%;
left: 50%;
margin-left: 520px;
padding-bottom: 50px;
}
.g-toolbar .toolbar-item {
position: relative;
margin: 0 0 10px 12px;
width: 40px;
height: 40px;
background: #4998f4;
border-radius: 4px;
-o-border-radius:4px;
-webkit-border-radius:4px;
-moz-border-radius:4px;
-ms-border-radius:4px;
cursor: pointer;
}

.g-toolbar .toolbar-item .item-tip-c {
position: absolute;
right: 45px;
text-align: center;
color: #fff;
overflow: hidden;
}
.u-spi {
display: inline-block;
width: 28px;
height: 28px;
margin: 5px 0 0 6px;
}
.u-043{background:url(../../images/subject/icons_30.png) no-repeat center center;}
.u-042{background:url(../../images/subject/icons_34.png) no-repeat center center;}
.u-025{background:url(../../images/subject/icons_38.png) no-repeat center center;}

.g-toolbar .toolbar-item .item-tip-c .item-box {
position: absolute;
right: -112px;
padding: 8px;
background: #d33737;
border-radius: 4px;
-o-border-radius:4px;
-webkit-border-radius:4px;
-moz-border-radius:4px;
-ms-border-radius:4px;
-ms-transition: right .3s cubic-bezier(0.17,.67,.88,1);
-moz-transition: right .3s cubic-bezier(0.17,.67,.88,1);
-webkit-transition: right .3s cubic-bezier(0.17,.67,.88,1);
transition: right .3s cubic-bezier(0.17,.67,.88,1);

}
.g-toolbar .toolbar-item .item-tip-c {
text-align: center;
color: #fff;
}
.item-tip{font-size:12px;}
.g-toolbar .toolbar-item .item-tip-c.item-tip-back .item-box{padding:12px 0px 11px 11px;}
.g-toolbar .toolbar-item .item-tip-c.item-tip-kefu .item-box{line-height: 14px;
    padding: 6px 0 6px 11px;}
.g-toolbar .toolbar-item:hover{background:#d33737;border-left:12px solid #d33737;margin:0 0 10px;}
.g-toolbar .toolbar-item:hover .item-tip-c{width:140px;height:140px;}

.g-toolbar .toolbar-item:hover .item-tip-c .item-box {right:0px;}
.w_footer_right img{margin-left:20px;}

.w_right_top{position: absolute;right:0px;top:0px;width:60px;height:60px;}

