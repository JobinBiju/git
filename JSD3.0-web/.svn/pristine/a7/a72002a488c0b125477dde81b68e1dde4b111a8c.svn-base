package com.ddtkj.mobile;

public class MobileException extends RuntimeException {

	private static final long serialVersionUID = 1L;

	public MobileException() {
        super();   
    }

    public MobileException(String s) {
        super(s);  
    }

    public MobileException(String s, Throwable throwable) {
        super(s, throwable);    
    }

    public MobileException(Throwable throwable) {
        super(throwable);   
    }
}
