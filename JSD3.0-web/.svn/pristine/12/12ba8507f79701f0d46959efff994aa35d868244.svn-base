<script>
	$(function(){
		function refreshPrize(id,type_val){
			var box = $(id);
			var ruleId = $("#id").val();
			var ruleUrl = "";
			if(type_val ==1)
			{
				ruleUrl = "/scoreAward/getGeAwardList.html?id="+ruleId+"&randID="+ escape(new Date());
			}
			else if(type_val ==2){
				ruleUrl = "/scoreAward/getGeAwardList.html?id="+ruleId+"&randID="+ escape(new Date())+"&level=level";
			}
			
			var str="";
			$.ajax({
				url:ruleUrl,
				error:function(){
					
				},
				success:function(data){
					var prize = data.gedata;
					var len = prize.length;
					for( var i = 0 ; i<len ; i++)
					{
						
						if(i%2==0){
							str=str+"<div class='list'><dl class='list_left'>"+
							"<dd><span class='name'>"+prize[i].userName+"</span><span class='date'>"+prize[i].time+"</span></dd>"+
							"<dd>"+prize[i].award+" </dd>"+
							"</dl>";
						}else{
							str=str+"<dl class='list_right'>"+
							"<dd><span class='name'>"+prize[i].userName+"</span><span class='date'>"+prize[i].time+"</span></dd>"+
							"<dd>"+prize[i].award+" </dd>"+
							"</dl></div>";
						}
					}
					box.html(str);
				}	
			})	
			
		}	
		refreshPrize($("#content_three"),1);
	});
	
	// 登录用户中奖记录
	$(function(){
		function refreshPrize(id){
			var box = $(id);
			var ruleId = $("#id").val();
			var ruleUrl = "/scoreAward/getMyGeAwardList.html?id="+ruleId+"&num=20"+"&randID="+ escape(new Date());
			var str = "";
			$.ajax({
				url:ruleUrl,
				error:function(){
				},
				success:function(data){
					var prize = data.mygedata;
					var len = prize.length;
					for( var i = 0 ; i<len ; i++)
					{
						str+="<li><span class='bannerspanl'>"+prize[i].award+"</span><span class='bannerspanr'>"+prize[i].time+"</span></li>";	
					}
					box.html(str);
				}	
			})	
		}	
		refreshPrize($(".award_record"));
	});
</script>	
	
