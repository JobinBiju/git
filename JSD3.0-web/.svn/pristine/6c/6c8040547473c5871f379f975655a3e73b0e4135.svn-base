@charset "utf-8";
@import url(comm.css);
*{margin:0;padding:0;font-family:microsoft Yahei;list-style:none;font-style:normal;font-weight:normal;}
img{border:none;}
body{background:#fff;}
.y_assessmentdlsbottom a{cursor:pointer;}
.y_details_con1leftul1 li {padding: 0 25px;}
.y_details_con1leftul1 li span em{font-size: 20px;}
.y_details_con2left{padding-top: 0;margin:0;margin-bottom:130px;border-top: 0;width:996px;}
.y_details_con2leftTitle li{width:199px;padding-bottom:10px;}
.y_details_con2leftTitle li.y_details_clicksli{width:198px;height: 23px;}
.y_details_con2leftTitle li span,.y_details_con2leftTitle li.y_details_clicksli span{width:199px;font-size: 16px}
.c_cliam_tender{width:880px;padding:36px 54px 0;}
.c_cliam_tender table{border-collapse:collapse}
.c_cliam_tender table tr{height:38px;border-bottom:2px solid #fff;color:#555;}
.c_cliam_tender table th{background:#3bafda;font-size:16px;color:#fff;font-weight:normal;border-right:2px solid #fff;}
.c_cliam_tender table td{text-align:center;border-right:2px solid #fff;}
.c_cliam_num{width:146px;}
.c_tender_man{width:216px;color:#06b5ff;}
.c_tender_money{width:200px;}
.c_tender_time{width:310px;}
.c_behave .c_cliam_num{width:190px;}
.c_behave .c_tender_man{width:150px;color:#555;}
.c_behave .c_tender_money{width:170px;}
.c_behave .c_tender_time{width:150px;}
.c_cliam_date{width:214px;}
.c_cliam_bot th,.c_cliam_bot td{width:218px;}
.c_cliam_bot .c_cliam_num{color:#06b5ff;}
.y_PageNums li{cursor:pointer;margin-left:7px;float: left;width: 30px;height: 30px;border: 1px solid #dddddd;line-height: 30px;text-align: center;color: #545454;font-size: 14px;border-radius: 2px;}
.y_details_con2right .y_PageNums li{width: 20px;height: 20px;line-height: 20px;font-size: 12px;}
.y_PageNums li.y_previousPage,.y_PageNums li.y_nextPage{width: 70px;height: 30px;border:1px solid #00a9e9;}
.y_details_con2right .y_PageNums li.y_previousPage,.y_details_con2right .y_PageNums li.y_nextPage{width: 50px;height: 20px;border:1px solid #00a9e9;}
.y_PageNums{margin: 30px auto;overflow: hidden;width: 392px;margin-bottom: 0;}
.y_details_con2right .y_PageNums{margin-top:30px;}
.y_PageNums li.y_previousPage:hover,.y_PageNums li.y_nextPage:hover{color: #00a9e9;}
.y_PageNums li.y_PageNumsclick{color: #fff;background-color: #00a9e9;}
.c_cliam_instruc{width:976px;margin:18px auto 30px;height:60px;line-height:60px;padding-left:20px;border:1px solid #DDDDDD;}
.c_cliam_instruc em{color:#555;font-style:normal;color:#0697da;font-size:18px;}
.c_cliam_intro{height:354px;border:1px solid #ddd;width:948px;padding:0 30px 0 20px;}
.c_cliam_detail{border:none;width:626px;}
.c_name_protocal{color:#0697da;margin:20px 0 54px 0;}
.c_name_protocal span,.c_name_protocal em{display:inline-block;font-size:18px;}
.c_name_protocal em{float:right;font-size:14px;cursor:pointer;}
.y_money_ps a{float:right;margin-right:26px;color:#06b5ff;cursor:pointer;}
.c_copies span{font-size:30px;}
.c_copies em{font-size:14px;}
.y_subtracts,.y_pluss{top:108px;}
.y_details_logas{margin:0;height: 34px;line-height: 34px}
.c_cliam_intro form .y_money_ps{color:#555;line-height:34px;}
.y_detailsinputs,.y_details_con1right form label{color:#888;}
.c_cliam_detail li{width:230px;float:left;font-size:14px;}
.c_cliam_detail li b{font-size:36px;display:block;color:#333;font-weight:normal;}
.c_tender_info dl{float:left;width:120px;margin-right:10px;margin-top:46px;}
.c_tender_info dl dd{line-height:30px;position:relative;}
.c_tender_info dd i{position:absolute;top: 8px;
left: 30px;display:inline-block;width:16px;height:16px;background:url(../../images/creditor/instruc.png) no-repeat;}
.y_tooltips {position: absolute;width: 170px;line-height: 22px;color: #555555;font-size: 12px;padding: 6px 12px;border: 1px solid #cbcbcb;border-radius: 3px;bottom: 36px;left: -66px;background: #fff;z-index: 1;}
.y_tooltips em {position: absolute;width: 17px;display: block;height: 10px;background: url(../../images/password/top_bottom.png) no-repeat;bottom: -10px;left: 95px;}
.no_login{margin:90px 0;text-align:center;font-size:18px;color:#666666;}
.no_login a{color:#0697da;font-size:18px;}
/*protocal*/
.modal-header {padding: 9px 15px;border-bottom: 1px solid #eee;}
.close {float: right;font-size: 20px;font-weight: 700;line-height: 20px;color: #000;text-shadow: 0 1px 0 #fff;opacity: .2;filter: alpha(opacity=20);}
.modal-header h3 ,.modal-body h4{margin: 0;line-height: 30px;}
.modal-body {position: relative;overflow-y: auto;max-height: 400px;padding: 15px;}
#Contract{opacity:1;background:#fff;z-index:2000;position: fixed;width: 800px;font-size:14px;top:15%;}
.modal-body p{padding-left: 20px;line-height: 26px;}
.modal-footer {padding: 14px 15px 15px;margin-bottom: 0;text-align: right;background-color: #f5f5f5;border-top: 1px solid #ddd;-webkit-border-radius: 0 0 6px 6px;-moz-border-radius: 0 0 6px 6px;-ms-border-radius: 0 0 6px 6px;-o-border-radius: 0 0 6px 6px;border-radius: 0 0 6px 6px;-webkit-box-shadow: inset 0 1px 0 #fff;-moz-box-shadow: inset 0 1px 0 #fff;-ms-box-shadow: inset 0 1px 0 #fff;-o-box-shadow: inset 0 1px 0 #fff;box-shadow: inset 0 1px 0 #fff;}
.modal-backdrop {position: fixed;top: 0;right: 0;bottom: 0;left: 0;z-index: 1040;background-color: #000;opacity: 0.5;filter: alpha(opacity=50);}
.c_buy_bj,.c_buy_bj1{position: fixed;top: 0;right: 0;bottom: 0;left: 0;z-index: 1040;background-color: #000;opacity: 0.5;filter: alpha(opacity=50);}
.btn-primary {border-style:none;background-color: #06b5ff;background-image: none;background-repeat: repeat-x;border: 0;filter: initial;-webkit-box-shadow: none;-moz-box-shadow: none;box-shadow: none;width: 60px;height: 30px;border-radius: 4px;color: #fff;font-family: microsoft Yahei;}
/*确认购买*/
.trueform{padding-top:18px;}
.trueform input{outline: none;}
.trueform p{line-height: 28px;height: 28px;}
.trueverify{width: 168px;padding-left:10px;height: 34px;border: 1px solid #dddddd;border-radius: 2px;}
.trueverifyimg{display:inline-block;width: 69px;height: 30px;background: url(../../images/password/true.jpg) no-repeat;position: relative;top: 10px;left: -71px;}
.trueform label{padding: 0 4px 0 8px;line-height: 20px;font-size: 14px;}
.trueagreement{display: inline-block;width: 105px;height: 20px;color: #ffffff;font-size: 12px;text-align: center;border-radius: 2px;background: #06b5ff;line-height: 20px;cursor: pointer;}
.trueformbottom{margin-top: 20px;}
.truebutton{width: 100px;height: 36px;background: #06b5ff;border: 0;border-radius: 3px;color: #fff;margin-right: 22px;cursor: pointer;margin-top: 14px;font-size:16px;}
.truereturn{width: 100px;height:36px;font-size:16px;background: #e3e3e3;border-radius: 3px;display: inline-block;text-align: center;line-height: 36px;margin-top: 14px;cursor: pointer;}
.c_cofirm_buy{display:none;position:fixed;width:460px;background:#fff;z-index:12000;padding:40px 30px 80px;font-size:14px;}
.c_cofirm_buy span{display:inline-block;width:90px;margin-right:20px;}
.c_cofirm_buy p{line-height:40px;}
.c_cofirm_buy i{font-style:normal;}
.c_cliam_btn{text-align:center;margin-top:30px;}
/*JSDP-35 gjh 2015-4-30 start*/
.c_cliam_close,.c_cliam_close1,.c_cliam_close21,.c_cliam_close2,.c_cliam_close3{width:34px;height:38px;position:absolute;right:-12px;top:-12px;background:url(../../images/authentications.png) no-repeat;}
/*购买成功*/
.c_buy_success,.c_buy_revocation,.c_buy_revocationing,.c_buy_revocationing1{width:520px;height:160px;position:absolute;z-index: 2000;background: #fff;}
.c_buy_revocationing,.c_buy_revocationing1,.c_buy_revocation,.c_buy_success{height: 200px;}
.c_success_instruc{width:110px;height:48px;line-height:48px;margin:50px auto 30px;padding-left:60px;background:url(../../images/password/success.png) no-repeat;font-size:16px;}
/*cliam_alert*/
.c_cliam_count{border:1px solid #ddd;width:168px;height:34px;padding-left:10px;}
.c_factor{width:94px;height:30px;border:1px solid #ddd;}
.c_factor:focus{outline:none;}
.c_buy_revocationing span,.c_buy_revocationing1 span,.c_cliamBT2,.c_cliamBT3,.c_cliamBT4{cursor: pointer;width: 80px;height: 34px;text-align: center;line-height: 34px;display: inline-block;background: #0697da;color: #fff;border-radius: 3px;margin-left: 20px;}
.c_cliamBT2,.c_cliamBT3,.c_cliamBT4{margin-left:220px;}
.c_buy_revocationing span.c_cliamBT1,.c_buy_revocationing1 span.c_cliamBT11{margin-left: 160px;}
/*JSDP-35 gjh 2015-4-30 end*/