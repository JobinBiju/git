.egg5 .egg_hover{left: 37px;top: 28px;width: 150px;height: 214px;}
.egg6 .egg_hover{width: 140px;top: 25px;left: 40px;height: 188px;}
.tamp_egg{width:840px;margin:0 auto;}
.tamp_left,.tamp_right{float:left;font-size:16px;color:#fff;line-height:30px;}
.tamp_left{width:254px;margin-right:140px;}
.tamp_right{width:420px;position:relative;}
.small_awl{display:inline-block;position:relative;top:20px;width:69px;height:57px;background:url(../../images/goldhammer/small_awl.png) no-repeat;}
.login_after{margin-top:50px;}
.award_record{height:90px;overflow:hidden;}
.award_record li{overflow:hidden;}
.award_record li span{float:left;color:#ffda65;}
.award_record li em{float:right;margin-right:30px;}
.tamp_right h3{font-size:16px;}
.award_btn{height:84px;width:26px;position:absolute;right: -18px;top: 35px;}
.award_btn span{display:block;width:26px;height:26px;background:url(../../images/goldhammer/record_btn.png) no-repeat;position:absolute;cursor:pointer;}
.award_up{top:0;}
.award_btn .award_down{bottom:0;background-position:0 -60px;}
.login_before a{display:block;width:254px;height:56px;background:url(../../images/goldhammer/tamp_egg.png) no-repeat;margin:70px auto 0;}
.active_rule{background-color:#ffda65;}
.rule_bj{width:100%;height:224px;background:url(../../images/goldhammer/rule_bj.png) no-repeat center 0;}
.rule_content{width:980px;margin:-40px auto;position:relative;}
.rule_title{position:absolute;width:98px;height:113px;background:url(../../images/goldhammer/active_intro.png) no-repeat;left:0;top:0;}
.active_kik{position:absolute;width:167px;height:212px;background:url(../../images/goldhammer/kik.jpg) no-repeat;right:0;top:8px;}
.rule_list{width:700px;padding:0 170px 0 108px;color:#f3422e;line-height:26px;font-size:17px;overflow:hidden;}
.active_content dl{float:left;overflow:hidden;width:585px;}
.active_content{overflow:hidden;margin-top:28px;}
.active_setting{overflow:hidden;}
.active_setting span,.active_content span{float:left;}
.jsd_more{display:block;width:254px;height:58px;background:url(../../images/goldhammer/award_btn.png) no-repeat 0 -200px;color:#ffda65;font-size:20px;text-align:center; line-height:48px;margin:30px 0 160px 138px;}
.jsd_more:hover{background-position:-404px -200px;}
.con_title{margin-top:2px;margin-right:20px;}
.red_packet{position:fixed;z-index:21;display:none;}
.award_goods{padding-top:104px;}
.award_goods p{text-align:center;color:#fffba5;font-size:56px;}
.award_goods p span{font-size:60px;font-weight:bold;}
.award_goods .goods_name{font-size:38px;margin-top:-60px;}
.award_goods .yg_card{margin-top:0px;}
.packet_bj{display:none;background: #000;opacity: 0.65;filter: alpha(opacity=65);position:absolute;top:0;width:100%;z-index:20;}
.award_goods{width:470px;height:190px;background:url(../../images/goldhammer/hb.png) no-repeat;}
.packet_close{position:absolute;text-indent:-9999em;top:2px;right:40px;cursor:pointer;width:20px;height:20px;}
.packet_btn{display:block;width:304px;height:74px;background:url(../../images/goldhammer/award_btn.png) no-repeat 0 -97px;margin:0 auto;}
.complete_data:hover{background-position:-403px -97px;}
.quick_user{background-position:0 0;}
.quick_user:hover{background-position:-403px 1px;}