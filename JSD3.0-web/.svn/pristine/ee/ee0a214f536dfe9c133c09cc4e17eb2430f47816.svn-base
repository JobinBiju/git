@charset "utf-8";
*{margin:0;padding:0;border:none;font-weight:normal;font-style:normal;list-style:none;}
a{text-decoration:none;color:#333;}
a:hover{color:##0198F1;}
body{min-width:1200px;font-size:14px;font-family:"微软雅黑";color:#333;background-color:#f3422e;position:relative;}
.banner{height:897px;width:100%;position:relative;}
.banner1{height:237px;background:url(../../images/goldhammer/banner1.jpg) no-repeat center 0;}
.banner2{height:115px;background:url(../../images/goldhammer/banner2.jpg) no-repeat center 0;}
.banner3{height:112px;background:url(../../images/goldhammer/banner3.jpg) no-repeat center 0;}
.banner4{height:133px;background:url(../../images/goldhammer/banner4.jpg) no-repeat center 0;}
