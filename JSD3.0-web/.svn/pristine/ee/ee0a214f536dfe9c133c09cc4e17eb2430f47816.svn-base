.tip{width:217px;height:152px;background:url(../../images/goldhammer/tip.png) no-repeat;position:absolute;z-index:10;top: -130px;right: -40px;display:none;}
.egg1 .tip{top: -106px;right: -40px;}
.egg5 .tip,.egg6 .tip{top: -115px;right: -87px;}
.egg4 .tip{top: -77px;right: -59px;}
.tip span{display:block;width:140px;height:63px;margin:0 auto;padding-top:44px;color:#666;}
.egg_hover{position: absolute;width: 160px;height: 227px;top: 53px;left: 50px;cursor:url(../../images/goldhammer/awls.cur),url(../../images/goldhammer/awls.cur),pointer;}
.egg2 .egg_hover{top: 20px;left: 30px;width:140px;height:190px;}
.egg3 .egg_hover{height: 220px;width: 148px;top: 28px;left: 39px;}
