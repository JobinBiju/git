.banner6{height:139px;background:url(../../images/goldhammer/banner6.jpg) no-repeat center 0;}
.awl_box{width:1200px;position:relative;margin:0 auto;}
.awl{width:300px;height:350px;background:url(../../images/goldhammer/awl.gif) no-repeat;position:absolute;right:188px;top:-93px;}
.main_content{width:100%;margin:30px auto 0;}
.block_one{height:938px;width:100%;background:url(../../images/goldhammer/award_bj.png) no-repeat center 0;}
.block_one h2{text-align:center;color:#fff;font-size:32px;padding-top:6px;}
.content_three{width:1000px;height:240px;margin:14px auto 0;position:relative;overflow:hidden;}
.list_all{border-radius: 4px;height:240px;color:#fff;margin-left: 88px;}
