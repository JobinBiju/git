						<label class="y_input_label">卡号</label>
						<!-- <input type="text" class="{required:true}" id="card_input"  maxlength="23" name="bankNo" onKeyUp="value=value.replace(/[^0-9]/g,'').replace(/\s/g,'').replace(/(\d{4})(?=\d)/g,'$1 ');"/><br/> -->
						<input type="text" class="{required:true}" id="card_input"  maxlength="19" name="bankNo" onKeyUp="value=value.replace(/[^0-9]/g,'').replace(/\s/g,'');"/><br/> 
						<span class="y_spans" style="position: relative;display:none;border: 1px solid #FA9494;left:98px;width: 244px;top: -23px;color: #E86969;background: #FCD1D1;" ></span>
						</div>
					   <div style="position:relative;">
