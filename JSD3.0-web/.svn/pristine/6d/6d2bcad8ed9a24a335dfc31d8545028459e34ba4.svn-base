  <script>
	$(function(){
		/*
		 *初始化省份选择框
		 * */
		$.ajax({
		    type:"get",
		    url:"/region/regionList.html?id="+Math.random(),
		    dataType:"json",
		    success:function(json){
				var provinces = "";
		    	 for(i=0;i<json.length;i++) {                                                                                                                                                                                        
		    		 
		    		  var province=json[i];
		    		  $(".province").append("<option value="+province.id+">"+province.name+"</option>");
		    	 }
				seajs.use("${webroot}${theme_dir}/media/js/jquery.select.region");
		    }
		});
		$($(".y_account_nav dd")[2]).addClass("c_invest_this");
	
		$("#card_input").keyup(function(){
			$(".card_number").show();
			$(".card_number").html($("#card_input").val().replace(/(\d{4})(?=\d)/g,'$1 '));
		})
		$("#card_input").blur(function(){
			$(".card_number").hide();
		})
	})
</script>
