<li class="record_conss_title">
	<dl>
		<dd class="record_dd5">积分类型</dd>
		<dd>操作积分</dd>
		<dd>可用积分</dd>
		<dd class="record_dd2">冻结积分</dd>
		<dd class="record_dd5">记录时间（分秒）</dd>
		<dd class="record_dd6" style="width:282px;">备注（详细说明）</dd>
	</dl>
</li>
{{#each data.list}}
<li>
	<dl>
		<dd class="record_dd5">{{scoreTypeName}}</dd>
		<dd>{{score}}</dd>
		<dd>{{validScore}}</dd>
		<dd class="record_dd2">{{freezeScore}}</dd>
		<dd class="record_dd5">{{transFormatDate addTime}}</dd>
		<dd class="record_dd6" style="width:282px;">{{{remark}}}</dd>
	</dl>
</li>						
{{/each}}	
