<script>
	$(function(){
		var numsapp=0;
 function moveapp(){
		numsapp++;
		if(numsapp==3){
			$(".y-app-bannerin121 ul").animate({marginLeft:-numsapp*276},1000,
				function(){
					numsapp=0;
					$($(".y-app-bannerin12 dl dd")[0]).addClass("appdd").siblings().removeClass("appdd");
					$(".y-app-bannerin121 ul").css("marginLeft","0");	
				}
			)
		}else{
			$($(".y-app-bannerin12 dl dd")[numsapp]).addClass("appdd").siblings().removeClass("appdd");
			$(".y-app-bannerin121 ul").animate({marginLeft:-numsapp*276},1000);
			
		}
	}


	var t = setInterval(moveapp,4000);
	$(".y-app-bannerin12 dl dd").click(function(){
		clearInterval(t);
		$(".y-app-bannerin121 ul").stop();
		$(this).addClass("appdd").siblings().removeClass("appdd");
		var index=$(this).index(".y-app-bannerin12 dl dd");
		$(".y-app-bannerin121 ul").animate({marginLeft:-index*276},1000);
		numsapp=index;
	})
	$(".y-app-bannerin12 dl").mouseleave(function(){
		t = setInterval(moveapp,4000);
	})
	$(".y-app-content ul li").mouseenter(function(){
		$(this).find(".app-titgif").attr("src",$(this).find(".app-titgif").attr("src").slice(0,-3)+"gif");
	})
	})

</script>
