{{#each data.list}}
<dl style="width:1200px;">
	<dd style="width:70px">{{transGuaranteeDateFormat repaymentYesTime}}<span>代偿日期<span></dd>
	<dd style="width:70px">{{{userNameFormat userName}}}<span>借款人<span></dd>
	<dd style="width:70px">{{MyDateDateFormat addTime}}<span>添加时间</span></dd>
	<dd style="width:100px">{{{logBorrowNameFun borrowName borrowId addTime}}}<span>标种标题<span></dd>
	<dd style="width:70px">{{borrowTypeName type}}<span>类型<span></dd>
	<dd style="width:70px">{{moneyFormat account}}元<span>金额<span></dd>
	<dd style="width:70px">{{apr}}%<span>年利率<span></dd>
	<dd style="width:70px">{{{timeLimitFormat type timeLimit borrowTimeType}}}<span>期限<span></dd>
	<dd style="width:100px">{{investPeriodFun borrowStyle period timeLimit}}<span>第几期/总期数<span></dd>
	<dd style="width:70px">{{MyDateDateFormat repaymentTime}}<span>应还日期<span></dd>
	<dd style="width:70px">{{capital}}天<span>代偿本金<span></dd>
	<dd style="width:70px">{{interest}}天<span>代偿利息<span></dd>
	<dd style="width:70px">{{lateDays}}天<span>逾期天数<span></dd>
	<dd style="width:70px">{{lateInterest}}<span>代偿罚息<span></dd>
	<dd style="width:70px">{{repaymentYesAccount}}<span>代偿总额<span></dd>
</dl>
{{/each}}